* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
a {
  text-decoration: none;
  color: inherit;
  display: block;
  -webkit-tap-highlight-color: transparent;
}
a:focus,
a:active,
a:hover,
a:visited {
  color: #5a5a5a;
  text-decoration: none;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
}
input {
  outline: none;
  border: none;
}
ul,
ol {
  list-style: none;
}
header .navbar {
  height: 44px;
  background-color: #ff841d;
}
header .navbar .navbar-header .return {
  height: 44px;
  position: relative;
  z-index: 100;
}
header .navbar .navbar-header .return span {
  color: #fff;
  font-size: 20px;
}
header .navbar h1 {
  text-align: center;
  font-size: 20px;
  font-weight: bold;
  position: absolute;
  width: 100%;
  margin: 0 auto;
  line-height: 50px;
  color: #fff;
  right: 0;
}
header .navbar .app-donwload {
  margin-top: 8px;
}
header .navbar .app-donwload img {
  width: 34px;
  display: block;
}
.navbar-default {
  margin-bottom: 0;
}
.foot_bar {
  width: 100%;
  height: 34px;
  line-height: 34px;
  margin-top: 10px;
  text-align: center;
}
.foot_bar ul li {
  float: left;
  width: 33.33%;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}
.foot_bar ul li + li {
  border-left: 1px solid #ccc;
}
.foot_bar ul li .icon-top {
  font-size: 15px;
}
footer {
  width: 100%;
  text-align: center;
  color: #5a5a5a;
  font-size: 12px;
  padding: 15px 0;
}
footer .foot_top a {
  display: inline-block;
}
footer .foot_top a:nth-of-type(1) {
  color: red;
}
/* 内容部分样式*/
.dis-content {
  padding: 0;
}
.dis-content .row {
  margin: 0;
}
.dis-content .row .col-xs-6 {
  padding: 0;
}
.dis-content .row .col-xs-6 .thumbnail {
  margin-bottom: 0;
}
.dis-content .row .col-xs-6 .thumbnail a {
  text-decoration: none;
}
.dis-content .row .col-xs-6 .thumbnail img {
  width: 140px;
  height: 140px;
}
.dis-content .row .col-xs-6 .thumbnail .caption h3,
.dis-content .row .col-xs-6 .thumbnail .caption p {
  font-size: 14px;
  overflow: hidden;
}
.dis-content .row .col-xs-6 .thumbnail .caption h3 {
  font-weight: bold;
  height: 36px;
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  line-height: 18px;
}
.dis-content .row .col-xs-6 .thumbnail .caption p {
  color: #ff6c00;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.dis-content .row .col-xs-6 .thumbnail .caption span {
  font-size: 12px;
  color: #ccc;
}
