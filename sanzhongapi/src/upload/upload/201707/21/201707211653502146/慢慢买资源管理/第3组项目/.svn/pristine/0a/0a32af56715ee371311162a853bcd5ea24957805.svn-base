.appbanner{background:#434343;position:relative;padding:10px;}
.appbanner:after{display: table;content: "";line-height: 0;clear: both;}
.appbanner .applogo{float:left;}
.appbanner .applogo img{width:64px;}
.appbanner .tit{float:left;color:#FFFFFF;font-size:16px; margin-left:10px;}
.appbanner .tit .subtit{font-size:12px;margin-top:4px;}
.appbanner .tit .subtit p{line-height:20px;color:#fee6c2;}
.appbanner .btn{float:left;}
.appbanner .btn a.a-close{display:block;width:20px; height:20px; position:absolute; top:10px; right:10px;background: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 28 28'><circle cx='14' cy='14' r='14' fill='%23000000'/><line stroke='%23ffffff' stroke-width='2' stroke-miterlimit='10' x1='8' y1='8' x2='20' y2='20'/><line fill='none' stroke='%23ffffff' stroke-width='2' stroke-miterlimit='10' x1='20' y1='8' x2='8' y2='20'/></svg>") no-repeat center; -webkit-background-size: contain;background-size: contain; }
.appbanner .btn a.a-install{display:block;width:80px; height:30px; line-height:30px;font-size:14px; color:#FFFFFF; background:#6db110;border-radius:4px; text-align:center; position:absolute; bottom:10px; right:10px;}

.appbannerc { position: relative; font-size:12px; margin-top:20px; }
.appbannerc .logo{ overflow:hidden;}
.appbannerc .btn { position: absolute; width: 80px; height: 30px; right: 10px; top: 8px; }
.appbannerc .btn a.a-install { position: static; }
.appbannerc .tit { font-size: 18px; float:none; padding-left:10px;overflow:hidden; }
.appbannerc .tit .subtit { margin-top: 4px; font-size:14px;}
.appbannerc .tit .subtit p { font-size:14px; }
.appbannerc .btn a.a-install{ height:28px; line-height:28px;}
.footer-cu{width: 100%; background-color: #FCFCFC; color: #666; position: fixed;right: 0;left: 0;bottom: 0;height: 44px; font-size: 10px; display: table; z-index:999;}
.footer-cu .footer-comment{ width: 30px;text-align: center; display: table-cell; vertical-align: middle; position:relative;}
.footer-cu .footer-comment a{ display:block; width:100%; height:100%; position:absolute; top:0px; left:0px;}
.footer-cu .footer-comment .icon-comment {height: 20px; text-align: center; line-height: 20px;}
.footer-cu .footer-comment .icon-comment:before{content: '\e62a'; color: #272636; font-size: 20px;}
.footer-cu .hover { -khtml-opacity: 0.8; opacity: 0.8; }
.footer-cu .foot-cubtn{display: table-cell; width: 70%; vertical-align: middle;}
.footer-cu .foot-cubtn ul{display: table; width: 100%; box-sizing: border-box;}
.footer-cu .foot-cubtn ul li{width: 50%;display: table-cell; box-sizing: border-box; padding: 0px; float: right;}
.footer-cu .foot-cubtn ul li.footer-haitao{padding: 0px 0 0 0px;}
.footer-cu .foot-cubtn ul li .tit{color: #FFFFFF; font-size: 16px; margin-left: 0px; display: inline-block; vertical-align: middle; line-height:normal;}
.footer-cu .foot-cubtn ul li .c{height: 44px; border-radius:0px; line-height: 44px; text-align: center; position: relative; top: 0px;}
.footer-cu .foot-cubtn .footer-gobuy .c{ background: #FF9000; }
.footer-cu .foot-cubtn .footer-gobuy .c a{display:block; width:100%; height:100%; position:absolute; top:0px; left:0px;}
.footer-cu .foot-cubtn .footer-gobuy{ width: 100%;}
.footer-cu .foot-cubtn .footer-gobuy .icon-gobuy { width: 18px; height: 18px; vertical-align: middle; display: inline-block; text-align: center; line-height: 18px;}
.footer-cu .foot-cubtn .footer-gobuy .icon-gobuy a{color:#ffffff;}
.footer-cu .foot-cubtn .footer-gobuy .icon-gobuy:before{content: '\e606'; color: #ffffff; font-size: 18px;}
