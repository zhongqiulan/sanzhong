// 功能跳转
.fn{
    height: 34px;
    margin-top: 6px;
    border-top:1px  solid #ccc;
    border-bottom:1px  solid #ccc;
    background: #F4F4F4;
    a{
         display:inline-block;
         width: 32%;
         font-size: 14px;
         color:#5a5a5a;
         text-align: center;
         height: 32px;
         line-height: 30px;
         border-left:1px  solid #ccc;
    }
}
