// 顶部
.head_logo{
    height:50px;
    padding-left:10px;
    background: #ff841d;
    .logo{
        width: 80%;
        height: 100%;
        padding: 12px;
        a{
             height: 100%;
            display: block;
             
            img{
                height: 100%;
            }
        }
    }
    .appdown{
        height: 100%;
        padding: 7px;
             a{
                height: 100%;
                display: block;
                
                img{
                    height: 100%;
            }
        }
        }

    
}

// 搜索框
.seek{
    // width: 100%;
    font-size: 16px;
    height: 50px;
    padding: 10px;
    position:relative; 
    border-bottom: 1px solid #e6e6e6;
     white-space: nowrap;
    .searcText{
        display:block;
        input{
        border:none;
        width: 90%;
        height: 30px;       
        line-height: 30px;
        padding-left:5%;
        border-radius:15px;
        background: #f1f1f1;
        cursor:auto;
         outline: none;
        
    }
    }
    
    .searcBtn>input{
        height: 30px;  
        line-height: 30px;     
        background: #ff841d;
        border:none;
        border-radius:15px;
        width: 63px;
        position:absolute;
        right:10px;
        top:10px;
        color:#fff;
    }
}

// 导航栏
.navss{
    .row{
        // margin-top: 15px;
        height: 33%;
        .col-xs-3{
            padding: 0;

            margin-top: 15px;
        }
        .thumbnail{
            border:none;
            margin-bottom: 0px;
            background: none;
            padding: 0;
            a{
                display: block;
                // width: 100%;
                img{
                    // width: 50px;
                    width: 50px;
                    min-width: 50px;
                    // height: 50px;
                }
                .caption{
                    // white-space:normal ;
                    white-space: nowrap;
                    display: block;
                    padding:0;
                    text-align: center;
                    font-size: 12px;
                }
            }
        
        }
    }
   
  
}

// 超值折扣推荐
.rebate{
    // height: 40px;
    line-height: 40px;
   
   
    position: relative;
   
    .recom{
            padding-left: 10px;
            padding-right: 10px;
            height: 40px;
            position: relative;
            background: #ff9000;
            
            a{
                display:block;
                height: 100%;
                width: 100%;
                color:#fff;
                font-size: 14px;
                font-weight:700;  
                
            }
            img{
                height: 50%;
                position: absolute;
                right: 10px;
                top: 10px;
            }
        }
   .recomdet{
               
            ul{
                list-style: none;
                padding: 0;
                li{
                    padding: 14px 8px;
                    height: 128px;
                    a{
                        display: block;
                        height: 100px;
                        padding-left: 100px;
                        position: relative;
                        .brand_Img{
                            position: absolute;
                            top: 0;
                            left:0;
                            img{
                                width: 100px;
                                height: 100px;
                            }
                        }
                        .title{
                            color:#333;
                            // word-break: break-all;
                            font-size: 15px;
                            line-height: 19px;
                            min-height:57px;
                            text-align: left;
                            
                            span{
                            color:#ff6c00;
                            display: inline;
                            }
                        }
                        .other{
                            color:#a1a1a1;
                            font-size: 12px;
                            span{
                                 white-space: nowrap;
                            }
                           
                        }
                    }     
                }
            }
    }
   

}

