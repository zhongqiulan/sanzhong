.navbar{
    background-color: #f4483e;
    height: 40px;
     position: relative;
    .navbar-header{
        .navbar-brand{
           width:40px;
            height: 40px;
            color:#fff;
        }
    };
    .navbar-center{
        font-size:26px;
        position:absolute;
        left:40%;
        top:5px;
        span{
            height: 40px;
            color:#fff;
        }
    }
    .app-donwload{
        margin-top: 5px;
        img{
           width:40px;
           height: 40px; 
        }
    }
};

