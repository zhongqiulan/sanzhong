@import 'common.less';
/* 内容部分样式*/
.dis-content{ 
  padding:0; 
  .row{ 
    margin: 0;
    .col-xs-6{ 
      padding:0;
      .thumbnail{
        margin-bottom: 0;
        a{
          text-decoration: none;
        }
        img{
          width: 140px;
          height: 140px;
        }
        .caption{
          h3,p{
            font-size: 14px;
            overflow: hidden;
          }
          h3{
            font-weight: bold;
            height: 36px;
            word-break: break-all;
            display: -webkit-box;
            -webkit-line-clamp: 2;
            -webkit-box-orient: vertical;
            overflow: hidden;
            line-height: 18px;
          }
          p{
            color: #ff6c00;
            text-overflow:ellipsis;
            white-space:nowrap;
            overflow: hidden;
          }
          span{
            font-size: 12px;
            color: #ccc;
          }
        }
      }
    }
  }
}