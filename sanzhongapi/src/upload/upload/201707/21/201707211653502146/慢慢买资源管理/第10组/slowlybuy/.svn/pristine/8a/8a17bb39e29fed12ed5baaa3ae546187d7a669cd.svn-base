div,
ul,
li,
a,
p,
ol,
h3,
em {
  margin: 0;
  border: 0;
  padding: 0;
  font-style: normal;
}
a {
  text-decoration: none;
}
ul {
  list-style: none;
}
/*左右浮动*/
.float_l {
  float: left;
}
.float_r {
  float: right;
}
/*左右浮动*/
/*清除浮动*/
.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
}
.clearfix:after {
  clear: both;
  height: 0;
}
/*兼容IE6/7*/
.clearfix {
  zoom: 1;
}
/*清除浮动*/
.bz_view,
header,
.search,
.bz_nav,
.bz_brans,
.bz_title,
.bz_list {
  height: 100%;
}
.bz_view {
  padding-top: 102px;
  position: relative;
}
.bz_view header {
  height: 102px;
  width: 100%;
  position: absolute;
  top: 0;
}
.bz_view header .logo {
  height: 51px;
  background-color: #ff841d;
}
.bz_view header .search {
  height: 51px;
}
.main .bz_nav {
  height: 36px;
  background-color: #ebebeb;
  background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#ebebeb));
  border-top: solid 1px #ebebeb;
  border-bottom: 1px solid #ccc;
  margin-bottom: 0px;
}
.main .bz_nav > li > a {
  text-decoration: none;
  color: #000;
}
.main .bz_brans {
  padding-top: 10px;
  background-color: #f2f2f2;
}
.main .bz_brans .bz_title {
  font-size: 16px;
  color: #874604;
  height: 40px;
  padding-left: 10px;
  line-height: 40px;
  margin: 0 10px 5px;
  border: 1px solid #ccc;
  background-color: #eee;
  margin-bottom: 0px;
}
.main .bz_brans .bz_list {
  padding: 0 10px;
}
.main .bz_brans .bz_list .fl_t a {
  height: 40px;
  background: url(../../images/arrow1.gif) right center no-repeat;
  line-height: 40px;
  color: #000;
  font-weight: bold;
  font-size: 16px;
  display: block;
  padding-left: 0px;
  border-bottom: 1px solid #ccc;
}
