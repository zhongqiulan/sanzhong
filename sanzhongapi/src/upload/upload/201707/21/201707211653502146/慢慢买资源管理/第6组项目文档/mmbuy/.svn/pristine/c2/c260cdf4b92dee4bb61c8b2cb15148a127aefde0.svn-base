/* 头部样式 */
/*初始化*/
* {
  margin: 0;
  padding: 0;
}
ul {
  list-style: none;
}
a {
  text-decoration: none;
  color: #000;
}
a:hover {
  color: #000;
  text-decoration: none;
}
.clearfix:before,
.clearfix:after {
  /*清除浮动，最好最标准的写法*/
  content: "";
  display: table;
}
.clearfix:after {
  clear: both;
}
.clearfix {
  *zoom: 1;
  /*IE/7/6*/
  /*兼容IE6下的写法*/
}
.navbar {
  border: none;
  margin-bottom: 0;
}
.container-fluid {
  height: 50px;
  background-color: #ff841d;
}
.container-fluid .navbar-header {
  padding-left: 10px;
}
.container-fluid .glyphicon {
  color: #fff;
  font-size: 20px;
  line-height: 50px;
}
.container-fluid .gsshopHead {
  font-size: 20px;
  font-weight: bold;
  display: inline-block;
  color: #fff;
  margin-left: 40%;
  line-height: 50px;
}
/* 商品导航部分 */
.gsshopnav-filter .left-tab {
  padding-right: 50px;
}
/* 商品展示部分 */
.gsshopnav-show {
  background-color: #f1f1f1;
  padding: 5px 6px;
}
.gsshopnav-show > ul > li {
  padding-right: 5px;
  padding-left: 5px;
  margin: 5px 0px;
}
.gsshopnav-show > ul > li > a {
  background-color: #fff;
  display: block;
}
.gsshopnav-show > ul > li > a > img {
  width: 100%;
}
.gsshopnav-show > ul > li > a .show-title {
  color: #333;
  word-break: break-all;
  padding: 0 6px;
  height: 14px;
  overflow: hidden;
  font-size: 12px;
  margin-top: 10px;
  line-height: 14px;
  font-weight: normal;
  text-align: left;
  margin-top: 6px;
}
.gsshopnav-show > ul > li > a > p {
  color: #f8601d;
  margin-bottom: 0;
  padding: 5px;
}
.gsshopnav-show > ul > li > a > p > .price {
  font-size: 20px;
}
