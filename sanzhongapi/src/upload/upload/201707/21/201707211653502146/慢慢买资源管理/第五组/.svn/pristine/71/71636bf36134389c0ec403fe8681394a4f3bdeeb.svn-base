*{
    padding: 0;
    margin: 0
}
a{
    color: #000;
    text-decoration: none;
    display: block;
}
input{
    border:none;
    background: none;
    outline: none;
}
ul{
    list-style:none;
}
a, abbr, address, b, blockquote, body, caption, cite, code, dd, del, dfn, div, dl, dt, em, fieldset, form, h1, h2, h3, h4, h5, h6, html, i, iframe, img, ins, kbd, label, legend, li, object, ol, p, pre, q, samp, small, span, strong, sub, sup, table, tbody, td, tfoot, th, thead, tr, ul, var {
    border: 0 none;
    font-size: inherit;
    color: inherit;
    margin: 0;
    padding: 0;
    vertical-align: baseline;
    max-height: 1000rem;
}

.clearfix:after{
    content: '';
    height: 0;
    visibility: hidden;
    display: block;
    clear: both;
}
.clearfix{
    zoom:1
}
.fl{
    float: left;
}
.fr{
    float: right;
}
