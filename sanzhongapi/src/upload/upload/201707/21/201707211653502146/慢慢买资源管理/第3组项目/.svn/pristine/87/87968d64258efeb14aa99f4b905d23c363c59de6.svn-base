* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
html,
body {
  height: 100%;
}
ol,
ul {
  list-style: none;
}
a {
  display: block;
  color: inherit;
  text-decoration: none;
}
img {
  display: block;
  width: 100%;
  vertical-align: middle;
}
p,
h3 {
  display: block;
}
input,
select,
button {
  display: block;
  height: 100%;
  width: 100%;
  border: none;
  outline: none;
  vertical-align: middle;
  webkit-tap-hightlight-color: transparent;
}
.clearfix:after {
  display: block;
  content: "";
  height: 0;
  clear: both;
  visibility: hidden;
}

.bottom_info .app-info a {
  line-height: 25px;
}
