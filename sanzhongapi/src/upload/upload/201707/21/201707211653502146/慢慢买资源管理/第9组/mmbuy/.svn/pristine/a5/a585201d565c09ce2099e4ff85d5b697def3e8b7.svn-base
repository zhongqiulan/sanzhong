/*公共样式*/
a {
  text-decoration: none;
  color: #000;
}
h3,
ul,
p {
  margin: 0;
  padding: 0;
}
ul,
ol {
  list-style: none;
}
em {
  font-style: normal;
}
.main {
  background-color: #f2f2f2;
}
.main .nav {
  width: 100%;
  height: 36px;
  line-height: 36px;
  font-size: 14px;
  padding-left: 8px;
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ccc;
  background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#ebebeb));
}
.main .nav a {
  color: #5a5a5a;
}
.main .model .model-title {
  color: #874604;
  border-top: 1px solid #c8c8c8;
  border-bottom: 1px solid #c8c8c8;
  padding-left: 10px;
}
.main .model .model-title h3 {
  height: 36px;
  line-height: 36px;
  font-size: 14px;
  font-weight: bold;
}
.main .model .model-list li {
  border-bottom: 1px solid #e7e7e7;
  padding: 10px;
}
.main .model .model-list li a em {
  display: inline-block;
  width: 20px;
  height: 20px;
  text-align: center;
  background: #c9c9c9;
  color: #FFFFFF;
  line-height: 20px;
  vertical-align: top;
  margin: 0 10px 0 0;
  font-size: 14px;
}
.main .model .model-list li a em.top1 {
  background-color: #f10e0e;
}
.main .model .model-list li a em.top2 {
  background-color: #ff9314;
}
.main .model .model-list li a em.top3 {
  background-color: #8adf5b;
}
.main .model .model-list li a .inner {
  display: inline-block;
}
.main .model .model-list li a .inner .tit {
  color: #000;
}
.main .model .model-list li a .inner p {
  font-size: 12px;
  color: #999;
}
