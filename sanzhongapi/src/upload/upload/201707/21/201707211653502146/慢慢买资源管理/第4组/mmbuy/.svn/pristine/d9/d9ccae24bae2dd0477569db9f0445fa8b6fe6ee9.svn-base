    .navbar {
        background-color: #ff841d;
    }

    .navbar .navbar-brand img {
        width: 187px;
    }

    .app-donwload img {
        height: 34px;
        margin-top: 8px;
    }

    .header-logo {
        width: 80%;
    }
    /*搜索栏*/

    #index_search {
        width: 100%;
        position: relative;
        margin-bottom: 1px;
    }

    #index_search .input1 {
        width: 90%;
        height: 30px;
        border-radius: 20px;
        border: 0;
        margin-left: 5px;
        padding-left: 10px;
        background-color: #f1f1f1;
    }

    #index_search .input2 {
        width: 60px;
        height: 30px;
        border-radius: 20px;
        padding-left: 10px;
        margin-right: 10px;
        background-color: #ff841d;
        color: #fff;
        position: absolute;
        right: 0;
        top: 0;
        border: 0;
    }

    .link {
        height: 36px;
        line-height: 16px;
        padding: 10px;
        margin-top: 16px;
        border-top: 1px solid #ccc;
        border-bottom: 1px solid #ccc;
        background: -webkit-gradient(linear, 0 0, 0 100%, from(#fff), to(#ebebeb));
        a {
            display: inline-block;
            color:#5a5a5a;
        }
    }

    main {
        background-color: #f2f2f2;
        overflow: hidden;
        .brand-title {
            width: 98%;
            padding: 10px;
            height: 36px;
            border: 1px solid #ccc;
            color: #874604;
            margin: 10px 1%;
        }
        >ul {
            margin: 10px;
            list-style: none;
            padding: 0;
            li {
                border-bottom: 1px solid #ccc;
                a {
                    font-size: 16px;
                    height: 42px;
                    width: 100%;
                    display: block;
                    line-height: 42px;
                    color: #000;
                    font-weight: 700;
                    position: relative;
                    i.down {
                        color: #949494;
                        line-height: 42px;
                        position: absolute;
                        right: 20px;
                    }
                }
            }
            
        }
    }