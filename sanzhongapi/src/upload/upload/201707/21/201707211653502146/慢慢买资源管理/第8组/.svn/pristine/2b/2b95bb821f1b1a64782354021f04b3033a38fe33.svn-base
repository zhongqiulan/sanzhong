* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

a {
    text-decoration: none;
    color: inherit;
    display: block;

    /* a标签点击高亮  */
    // -webkit-tap-highlight-color: transparent;
}

ul {
    list-style: none;
}

input {
    outline: none;
    border: none;
    display: block;
    width: 100%;
    height: 100%;
}

img {
    display: block;
    width: 100%;
}

.clearfix:after {
    display: block;
    content: "";
    clear: both;
    height: 0;
}