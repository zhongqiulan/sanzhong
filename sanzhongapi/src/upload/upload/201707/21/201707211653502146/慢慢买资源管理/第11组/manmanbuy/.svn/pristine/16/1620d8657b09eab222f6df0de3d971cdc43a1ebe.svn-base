    .hdh {
        margin: 0;
        padding: 0;
        .top {
            .navbar-default {
                width: 100%;
                height: 40px;
                margin-bottom: 0;
                box-sizing: border-box;
                background: #ff841d;
                .navbar-brand {
                    span {
                        width: 17px;
                        height: 17px;
                        color: #fff;
                    }
                }
                .form-group {
                    h1 {
                        margin: 0;
                        padding: 0;
                        font-size: 20px;
                        font-weight: bold;
                        color: #fff;
                        text-align: center;
                        line-height: 50px;
                    }
                }
                .pull-right {
                    @media screen and (max-width: 300px) {
                        display: none;
                    }
                    padding-top:8px;
                    img {
                        width: 34px;
                    }
                }
            }
        }
        .main {
            h3 {
                font-weight: normal;
                font-size: 18px;
                // margin: 10px auto 5px auto;
                font-weight: 800;
                text-align: left;
                text-align-last: left;
                word-break: break-all;
                color: #5a5a5a;
                line-height: normal;
            }
        }
    }