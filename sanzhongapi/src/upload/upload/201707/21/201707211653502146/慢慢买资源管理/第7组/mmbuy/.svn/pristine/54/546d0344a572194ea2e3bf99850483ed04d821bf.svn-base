//获取url后面Id号的封装函数；
    function queryString(){
      var search = location.search;
      search = search.slice(1); 
      var searchArr = search.split('&');  
      var obj = {};  
      for(var i=0;i<searchArr.length;i++){
        var temp =  searchArr[i].split('='); 
        obj[temp[0]] = temp[1];
      }
      return obj; 
    }
