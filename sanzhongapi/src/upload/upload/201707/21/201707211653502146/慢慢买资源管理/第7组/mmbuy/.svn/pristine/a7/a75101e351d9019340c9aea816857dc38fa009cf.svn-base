                        <p>{{ value.name }}</p>
                    </div>
                    <!--</a>-->
                </div>
            </div>
            {{/if}} {{/each}}

            <div class="collapse" id="collapseExample">
                <div class="well">
                    {{ each result as value i }} {{if i> 7}}
