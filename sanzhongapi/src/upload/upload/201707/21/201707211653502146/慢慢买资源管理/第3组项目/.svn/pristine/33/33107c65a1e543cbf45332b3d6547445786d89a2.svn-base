#aspnetForm .details-content {
  padding: 4px 10px ;
  margin: 0 auto;
}
#aspnetForm .details-content .det-title {
  color: #5a5a5a;
  margin: 10px auto 5px auto;
  text-align: left;
  word-break: break-all;
  font-weight: 800;
  font-size: 18px;
  line-height: normal;
}
#aspnetForm .details-content .det-info {
  margin: 12px auto;
  font-size: 12px;
  color: #999999;
  line-height: 24px;
}
#aspnetForm .details-content .det-info > span {
  margin-right: 4px;
}
#aspnetForm .details-content #det-content .right-img {
  float: right;
}
#aspnetForm .details-content #det-content .right-img > img {
  width: 120px;
  height: 120px;
}
#aspnetForm .details-content #det-content .left-font {
  font-size: 16px;
  word-break: break-all;
  color: #606060;
  line-height: 24px;
  padding-bottom: 20px;
}
#aspnetForm .details-content .golink {
  height: 40px;
  line-height: 40px;
}
#aspnetForm .details-content .golink > a {
  margin: 20px 10px ;
  background-color: #ff9000;
  text-align: center;
  font-size: 14px;
  color: #ffffff;
}
#aspnetForm .comment-box {
  position: relative;
}
#aspnetForm .comment-box .com-tit {
  font-size: 16px;
  padding: 6px 8px;
}
#aspnetForm .comment-box .com-replay {
  padding: 6px 8px;
}
#aspnetForm .comment-box .com-replay .form {
  border: 2px solid #ECEAE8;
  border-radius: 2px;
  padding: 4px 0 0 4px;
  background-color: #fff;
  position: relative;
  width: 98%;
  margin: 0px auto;
}
#aspnetForm .comment-box .com-replay .form > textarea {
  width: 100%;
  resize: none;
  overflow-x: hidden;
  overflow-y: auto;
  border: none;
  line-height: 22px;
  font-size: 14px;
  outline: 0;
  color: #666;
  height: 68px;
}
#aspnetForm .comment-box .com-ctrl {
  position: relative;
}
#aspnetForm .comment-box .com-ctrl > input {
  position: absolute;
  bottom: 8px;
  right: 19px;
  background: #ff841d;
  border: none;
  border: 2px solid #ff841d;
  border-left: none;
  border-top: none;
  padding: 0px;
  width: 100px;
  height: 39px;
  color: #fff;
  outline: 0;
  border-radius: 0 0 2px 0;
  font-size: 16px;
}
#aspnetForm .comment-box #new-com {
  font-size: 16px;
  padding: 6px 8px;
}
#aspnetForm .com-list {
  display: block;
}
#aspnetForm .com-list ul li {
  padding: 18px 10px;
  background-color: #FFFFFF;
}
#aspnetForm .com-list ul li .user-img {
  width: 40px;
  float: left;
  overflow: hidden;
}
#aspnetForm .com-list ul li .user-img > img {
  width: 40px;
  background-position: top;
  border-radius: 20px;
  height: 40px;
}
#aspnetForm .com-list ul li .user-con {
  overflow: hidden;
  padding-left: 10px;
}
#aspnetForm .com-list ul li .user-con .name {
  margin-bottom: 6px;
}
#aspnetForm .com-list ul li .user-con .name > user-name {
  color: #5997c0;
  font-size: 13px !important;
  float: left;
}
#aspnetForm .com-list ul li .user-con .name > user-time {
  color: #999999;
  font-size: 13px !important;
  float: right;
}
#aspnetForm .com-list ul li .user-con .user-content {
  word-break: break-all;
  font-size: 15px !important;
  color: #606060;
  line-height: 20px;
}
.det-menu {
  height: 36px;
  background-color: #ebebeb;
  border-top: solid 1px #ebebeb;
  border-bottom: 1px solid #ccc;
  line-height: 36px;
  padding-left: 8px;
  font-size: 14px;
}
.det-menu ul li {
  float: left;
}
.footer .footer-top {
  background: #efefef;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  height: 34px;
  line-height: 34px;
  margin-top: 25px;
}
.footer .footer-top ul li {
  float: left;
  width: 33.333%;
}
.footer .footer-top ul li > a {
  box-sizing: border-box;
  font-size: 14px;
  border-right: 1px solid #c4c4c4;
  text-align: center;
}
