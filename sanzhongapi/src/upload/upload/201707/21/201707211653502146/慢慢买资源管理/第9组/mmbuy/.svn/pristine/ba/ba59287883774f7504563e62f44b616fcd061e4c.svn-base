body {
  overflow-x: hidden;
}
body a {
  color: #000;
}
.head {
  height: 44px;
  line-height: 44px;
  position: relative;
  background-color: #e4393c;
  margin: 0;
}
.head a {
  display: block;
  width: 50px;
  height: 44px;
  color: #fff;
  font-size: 20px;
  text-align: center;
  text-decoration: none;
}
.head .back {
  position: absolute;
  left: 0;
  top: 0;
}
.head h1 {
  width: 100%;
  margin: 0;
  color: #fff;
  font-size: 20px;
  font-weight: bold;
  text-align: center;
  line-height: 44px;
}
.head .home {
  position: absolute;
  right: 0;
  top: 0;
}
.head .home img {
  width: 60%;
}
.nav-tabs {
  height: 38px;
  overflow: hidden;
  width: 150%;
}
.nav-tabs li {
  overflow: hidden;
  display: inline-block;
  height: 38px;
  line-height: 38px;
}
.nav-tabs li.active {
  border-bottom: 2px solid #f00;
}
.tab-content .tab-pane .bc_goods_list {
  list-style: none;
  padding: 0;
  margin-top: 10px;
}
.tab-content .tab-pane .bc_goods_list li {
  padding: 10px 5px;
  margin-bottom: 10px;
  border: 1px solid #ddd;
  width: 100%;
  position: relative;
}
.tab-content .tab-pane .bc_goods_list li .img {
  position: absolute;
  left: 0;
  top: 10px;
}
.tab-content .tab-pane .bc_goods_list li .img img {
  height: 130px;
  width: 130px;
}
.tab-content .tab-pane .bc_goods_list li .text {
  width: 100%;
  padding-left: 140px;
}
.tab-content .tab-pane .bc_goods_list li .text .i {
  height: 70px;
}
.tab-content .tab-pane .bc_goods_list li .text .i .pr {
  vertical-align: middle;
  font-style: normal;
  color: red;
  font-size: 20px;
}
.tab-content .tab-pane .bc_goods_list li .text .i .pr i {
  font-style: normal;
  font-size: 16px;
  margin-right: 4px;
}
.tab-content .tab-pane .bc_goods_list li .text .i .pr em {
  font-size: 12px;
}
.tab-content .tab-pane .bc_goods_list li .text .i .pr del {
  color: #a8a8a8;
  font-size: 12px;
}
.tab-content .tab-pane .bc_goods_list li .text .o {
  height: 60px;
}
.tab-content .tab-pane .bc_goods_list li .text .o .b a:nth-child(1) {
  display: block;
  width: 80px;
  height: 38px;
  color: #fff;
  background-color: #e4393c;
  padding: 0px 10px;
  text-align: center;
  line-height: 38px;
  border-radius: 4px;
  float: right;
}
.tab-content .tab-pane .bc_goods_list li .text .o .b a:nth-child(2) {
  float: left;
  border: 1px dotted #e4393c;
  height: 26px;
  line-height: 26px;
  text-align: center;
  display: block;
  padding: 0px 5px;
  font-size: 12px;
  color: #e4393c;
}
.tab-content .tab-pane .bc_goods_list li .text .o .bar {
  font-size: 12px;
  color: #666;
}
.tjzk {
  width: 100%;
  height: 40px;
  line-height: 40px;
  background-color: #ff9000;
  padding: 0 10px;
  position: relative;
}
.tjzk h3 {
  color: #fff;
  font-size: 18px;
  line-height: 40px;
}
.tjzk .icon-xiangyou {
  font-size: 24px;
  position: absolute;
  right: 8px;
  top: 0px;
  color: #fff;
}
.foot_bar {
  width: 100%;
  height: 34px;
  line-height: 34px;
  margin-top: 10px;
  text-align: center;
}
.foot_bar ul {
  list-style: none;
  padding-left: 0;
}
.foot_bar ul li {
  float: left;
  width: 33.33%;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}
.foot_bar ul li + li {
  border-left: 1px solid #ccc;
}
.foot_bar ul li .icon-top {
  font-size: 15px;
}
footer {
  width: 100%;
  text-align: center;
  color: #5a5a5a;
  font-size: 12px;
  padding: 15px 0;
}
footer .foot_top a {
  display: inline-block;
}
footer .foot_top a:nth-of-type(1) {
  color: red;
}
