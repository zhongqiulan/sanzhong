* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
a {
  text-decoration: none;
  color: inherit;
  display: block;
  /* a标签点击高亮  */
}
ul {
  list-style: none;
}
input {
  outline: none;
  border: none;
  display: block;
  width: 100%;
  height: 100%;
}
img {
  display: block;
  width: 100%;
}
.clearfix:after {
  display: block;
  content: "";
  clear: both;
  height: 0;
}
.save_box {
  background-color: #f2f2f2;
}
.header {
  background-color: #ff841d;
  height: 44px;
  width: 100%;
  position: relative;
}
.header .back {
  width: 50px;
  height: 100%;
  position: absolute;
  left: 10px;
}
.header .back a {
  width: 100%;
  height: 100%;
  font-size: 30px;
}
.header .back a > i {
  color: #fff;
  font-size: 18px;
  font-weight: bold;
}
.header h1 {
  text-align: center;
  line-height: 44px;
  font-size: 20px;
  color: #fff;
  margin: 0;
}
.header .home {
  position: absolute;
  right: 0;
  top: 6px;
}
.header .home img {
  width: 32px;
  height: 32px;
}
.content {
  margin: 0 auto;
  max-width: 960px;
}
.content .ul_content {
  margin: 0 auto;
  text-align: left;
  background-color: #fff;
  font-size: 14px;
}
.content .ul_content li {
  padding: 14px 8px;
  border-bottom: 1px solid #e0e0e0;
}
.content .ul_content li a {
  text-decoration: none;
  position: relative;
}
.content .ul_content li a .pic {
  float: left;
}
.content .ul_content li a .pic > img {
  width: 100px;
  height: 100px;
}
.content .ul_content li a .info {
  padding-left: 126px;
}
.content .ul_content li a .info .title {
  color: #333333;
  font-size: 15px;
  line-height: 19px;
  min-height: 57px;
}
.content .ul_content li a .info .title span {
  color: #ff6c00;
}
.content .ul_content li a .info .other {
  margin-top: 8px;
  color: #a1a1a1;
  font-size: 12px;
}
.content .ul_content li a .info .other .mall {
  float: left;
}
.content .ul_content li a .info .other .comment {
  float: right;
}
.content .ul_content li a .info .other .comment > i {
  width: 14px;
  height: 15px;
  margin-right: 5px;
  font-size: 12px;
}
.content .page {
  background-color: #f2f2f2;
  height: 80px;
  padding-top: 23px;
}
.content .page .page_c {
  height: 34px;
  position: relative;
}
.content .page .page_c .pre_page {
  position: absolute;
  top: 0;
  left: 10px;
}
.content .page .page_c .pre_page button {
  padding: 6px 20px;
  background-color: #d4d4d4;
  border-radius: 8px;
}
.content .page .page_c .search {
  margin: 0 auto;
  width: 100px;
}
.content .page .page_c .search select {
  height: 34px;
  width: 100px;
}
.content .page .page_c .next_page {
  position: absolute;
  top: 0;
  right: 10px;
}
.content .page .page_c .next_page button {
  padding: 6px 20px;
  background-color: #d4d4d4;
  border-radius: 8px;
}
.save_nav {
  height: 36px;
  background-color: #ebebeb;
  padding-left: 10px;
  border-top: 1px solid  #ebebeb;
  border-bottom: 1px solid #ccc;
}
.save_nav a {
  display: inline-block;
}
.save_nav a span {
  line-height: 36px;
}
.footer {
  padding-top: 20px;
}
.footer .footer_top a {
  float: left;
  width: 33.33%;
  text-align: center;
  height: 34px;
  line-height: 34px;
  background-color: #f2f2f2;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  border-left: 1px solid #c4c4c4;
}
.footer .footer_top a > img {
  display: inline-block;
  width: 15px;
  height: 15px;
  margin: 0 auto;
}
.footer .footer_info {
  font-size: 12px;
  padding: 14px 0;
}
.footer .footer_info .toot_app {
  text-align: center;
  color: #666;
}
.footer .footer_info .toot_app a {
  display: inline-block;
}
.footer .footer_info .toot_app a.app_red {
  color: red;
}
.footer .footer_info .toot_app a.app_self {
  color: #000;
}
.footer .footer_info .foot_http {
  text-align: center;
  color: #666;
}
.footer .footer_info .foot_http a > img {
  width: 60px;
  position: fixed;
  right: 0;
  bottom: 20px;
}
