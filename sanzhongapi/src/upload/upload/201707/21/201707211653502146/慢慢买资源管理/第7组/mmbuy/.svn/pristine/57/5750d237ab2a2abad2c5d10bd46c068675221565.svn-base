    <script src="../../libs/artTemplate/template.js"></script>
    <script src="../../libs/jquery-cookie/jquery.cookie.js"></script> 
    <script src="../../js/utils.js"></script>
    <script src="../../js/category/ductlist.js"></script>

    <!-- 渲染标题栏   -->
    <script type="text/template" id="ductListTpl">
        {{each result as v i}}
        <input type="hidden" name="v.brandName" value="{{v.brandName}}">
        <div class="media" data-productListId={{v.productListId}}>
            <div class="media-left media-middle">
                <a href="#">{{#v.productImg}}</a>
            </div>
            <div class="media-body">
                <div  class="media-heading">{{v.productName}}</div>
                <p>{{v.productPrice}}</p>
                <span>{{v.productQuote}}</span>
                <span>{{v.productCom}}</span>
            </div>
            <a href="./product.html?productid={{v.productId}}"></a>
        </div>
        {{/each}}
    </script>

