<body>
    
    <div class="header-wrap">
        <header>
            <div class="back">
                <a href="../index.html">
                    <i class="iconfont icon-houtui"></i>
                </a>
            </div>
            <div class="title">
                <h2>凑单品</h2>
            </div>
            <div class="app-download">
                <a href="javascript:;" alt=""></a>
            </div>
        </header>
    </div>

    <div class="nav-sel">
        <ul>
            <li>
                <a href="javascript:;" class="shop-btn">
                    京东
                    <i class="iconfont icon-shangsanjiao"></i>
                </a>
            </li>
            <li>
                <a href="javascript:;" class="area-btn">
                    华北
                    <i class="iconfont icon-shangsanjiao"></i>
                </a>
            </li>
            <li>
                <a href="javascript:;">
                    全部价格
                    <i class="iconfont icon-shangsanjiao"></i>
                </a>
            </li>
            <li class="search">
                <a href="javascript:;">
                    <i class="iconfont icon-sousuo-liebiao"></i>
                </a>
            </li>
        </ul>
        <div class="popup">
            <div class="shop">
                
            </div>
            <div class="area">
                
            </div>
            <div class="price">
                <ul>
                    <li><a href="javascript:;">写死的价格</a></li>
                    <li><a href="javascript:;">写死的价格</a></li>
                    <li><a href="javascript:;">写死的价格</a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="prolist" id="list">
        
