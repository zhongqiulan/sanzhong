<!--店铺模板-->
<script type="text/template" id="tpl-shop">
    <ul>
        {{each result as v i}}
        <li><a href="javascript:;" data-id="{{v.shopId}}">{{v.shopName}}</a><i class="iconfont icon-ok"></i></li>
        {{/each}}
    </ul>
</script>

<!--地区模板-->
<script type="text/template" id="tpl-area">
    <ul>
        {{each result as v i}}
        <li><a href="javascript:;" data-id="{{v.areaId}}">{{v.areaName}}</a><i class="iconfont icon-ok"></i></li>
        {{/each}}
    </ul>
</script>

<!--列表模板-->
<script type="text/template" id="tpl-list">
    <ul>
        {{each result as v i}}
        <li>
            <img src="{{v.productImg}}" alt="">
            <p>{{v.productName}}</p>
            <div class="clearfix">
                <span class="price">{{v.productPrice}}</span>
                <a href="javascript:;">去凑单</a>
            </div>
        </li>
        {{/each}}
    </ul>
</script>

<script src="../libs/zepto/zepto.js"></script>
