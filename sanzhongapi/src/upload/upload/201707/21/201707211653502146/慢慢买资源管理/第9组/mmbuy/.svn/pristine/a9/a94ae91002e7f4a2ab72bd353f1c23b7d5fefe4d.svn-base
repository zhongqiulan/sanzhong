<script type="text/template" id="save_list_tmp">
    <h3>{{productName}}</h3>
    <h4>{{productPinkage}}</h4>
    <div class="discount_content">
        <span class="jd_mall">{{productFrom}}</span>
        <span class="jd_time">{{productTime}}</span>
        <span class="jd_info">{{productTips}}</span>
        <a href="javascript:;">
                <span class="jd_commits">{{productComCount}}</span>
            </a>
    </div>
    <div class="goods_pic">
        {{#productImgSm}}
    </div>
    <p class="this_type">{{productInfo}}
    </p>
    <p class="current_price">
        {{productInfo1}}
    </p>
    <div class="discount_pic_top">
        {{#productImgLg}}
    </div>
    <div class="discount_pic_bot">
        {{#productImg3}}
    </div>
    <a href="javascript:;">
        <div class="buy_btn">
            <i class="iconfont icon-gouwuche"></i>前往购买
        </div>
    </a>
    <div class="container">
         {{#productComment}}
    </div>
</script>
<script>
    var search = location.search;
    console.log(search);
    search = search.slice(1);
    console.log(search);
    var searcharr = search.split('&');
    console.log(searcharr);  //["tc_id=12"]
    var obj = {};
    for (var i = 0; i < searcharr.length; i++) {
        var temp = searcharr[i].split('=');
        console.log(temp);      //["tc_id", "12"]
        obj[temp[0]] = temp[1];
        //22 
    }
    console.log(obj.productid);
    $.ajax({
        url: "http://157.122.54.189:9090/api/getmoneyctrlproduct",
        dataType: 'JSON',
        type: "GET",
        data: {
            productid: obj.productid
        },
        success: function (data) {
            console.log(999);
            console.log(data);
            console.log(data.result);
            var htmlstr = template("save_list_tmp", data.result[0]);
            $(".buy_details").html(htmlstr);
        }
    })

</script>