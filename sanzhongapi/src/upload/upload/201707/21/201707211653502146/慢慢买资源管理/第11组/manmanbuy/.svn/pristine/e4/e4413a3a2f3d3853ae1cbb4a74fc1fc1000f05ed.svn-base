ul,
ol,
li {
    list-style: none;
}


.ydb .head {
    background: #ff841d;
    color: #FFFFFF !important;
    height: 44px;
    width: 100%;
    box-sizing: border-box;
}

.ydb .head .back {
    height: 44px;
    width: 10%;
    text-align: center;
    line-height: 44px;
    font-size: 17px;
    float: left;
}

.ydb .head .back a {
    color: #fff;
    width: 100%;
    margin: 0 auto;
}

.ydb .head .head-title {
    width: 80%;
    float: left;
}

.ydb .head h1 {
    font-size: 20px;
    font-weight: bold;
    padding: 0px;
    margin: 0px 48px;
    text-align: center;
    line-height: 44px;
    display: block;
}

.ydb .head .app-download {
    width: 10%;
    height: 44px;
    /*background-color: red;*/
    float: right;
}

.ydb .head .app-download img {
    height: 34px;
    margin-top: 8px;
}

.ydb .main {
    max-width: 960px;
    margin: 0px auto;
}

.ydb .main .m-product {
    background-color: #ffffff;
    text-align: left;
    font-size: 14px;
    max-width: 960px;
    margin: 0px auto;
}

.m-product ul {
    padding: 0px;
}

.m-product ul li {
    padding: 14px 8px;
    position: relative;
}

.m-product .pic {
    float: left;
    width: 110px;
}

.m-product .pic img {
    width: 100px;
    height: 100px;
}

.m-product .info {
    padding-left: 90px;
    font-size: 14px;
    line-height: 24px;
}

.m-product .info .title {
    color: #333333;
    word-break: break-all;
    font-size: 15px;
    line-height: 19px;
    min-height: 57px;
    text-align: left;
    font-weight: normal;
}

.m-product .info .title span {
    color: #ff6c00;
    display: inline;
}

.m-product .info .other {
    margin-top: 8px;
    color: #a1a1a1;
}

.m-product .info .other .mall {
    color: #a1a1a1;
    font-size: 12px;
    float: left;
}

.m-product .info .other .mall {
    display: inline-block;
}

.m-product .info .other .comment {
    color: #a1a1a1;
    font-size: 12px;
    float: right;
    /*display: inline-block;*/
}

.m-product .info .other .comment i {
    background: url('../../uploads/moneyctrl/evaluate.png');
    width: 14px;
    height: 15px;
    display: inline-block;
    background-size: auto 100%;
    background-repeat: no-repeat;
    background-position: center;
    vertical-align: middle;
    margin-right: 4px;
}

.ydb .pages {
    width: 100%;
    text-align: center;
    /*margin-top: 20px;*/
    /*margin-bottom: 20px;*/
    padding:20px 0;
    background-color: rgb(242, 242,242)
}

table {
    border-collapse: collapse;
    border-spacing: 0;
    width: 100%;
}

.ydb .pages a {
    box-sizing: border-box;
    background-color: #5d0707;
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#f1f1f1), to(#d4d4d4));
    border-bottom: 1px solid #bababa;
    box-shadow: 0 0 1px #666;
    border-radius: 5px;
    text-align: center;
    font-size: 14px;
    padding: 8px 20px;
    color: #000;
}

.pages span {
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#f1f1f1), to(#d4d4d4));
       border-radius: 5px;
       border:1px solid #bababa;
       width: 82px;
    font-size: 16px;
    display: inline-block;
    /*padding: 16px 15px;*/
    height: 37px;
    text-align: center;
    line-height: 37px;

.direction{
    height: 37px;
    background: -webkit-gradient(linear, 0 0, 0 100%,from(#ffffff),to(#ebebeb));
    border-top: solid 1px #ebebeb;
    border-bottom: 1px solid #ccc;
    line-height: 36px;
    padding-left: 8px;
    font-size: 14px;
    padding-bottom: 20px;
}

.direction a{
    color: #000;
}



footer {
    background: #efefef;
}

footer .menu ul {
    padding: 0;
    margin: 0;
}

footer .menu li {
    height: 35px;
    line-height: 35px;
    text-align: center;
    border-bottom: 1px solid #c4c4c4;
    border-top: 1px solid #c4c4c4;
    float: left;
    width: 33.3%;
}

footer .menu li:nth-of-type(2) {
    border: 1px solid #c4c4c4;
}

footer .menu li a {
    display: block;
    color: #000;
}

footer .xinxi {
    font-size: 13px;
    text-align: center;
    padding: 15px 0;
}

footer .xinxi p {
    margin: 0;
    color: #747474;
}

footer .xinxi p a:nth-of-type(1) {
    color: red;
}

footer .xinxi p a:nth-of-type(2) {
    color: #000;
}

.mmb-app{
        position: fixed;
    right: 0;
    bottom: 30px;
    z-index: 999;
}

.mmb-app img{
     width: 60px;
     height: 67px;
}