

ul,ol {
    list-style: none;
    padding:none;
}
a {
    color:#333333;
}
/* 
 为头部留出位置   */
 .saveMoneyHeader {
    height: 44px;
    background-color: #ff841d;
};    


/* 页面主体的样式 */

.main {
    padding:0;
}  
.ulist123 {
     position: relative; 
     padding: 0px; 
}
a.listLink {
    display: block;
    position: relative;
    padding-left: 100px;
    height:100px;
}
li.list {
    padding:14px;
}
a.listLink .left {
    width:100px;
    height: 100px;
    position: absolute;
    left:0;
    top:0;
}
a.listLink .left>img {
    width:100%;
}


a.listLink .right {
    width:100%;
    height:100%;
}  
.right>.title {
    height: 55px;
}
.right .title span.price {
    color:#ff841d;
}
.right>.info {
    color:#ccc;
}


/*按钮  */

.pages {
    background-color: #f2f2f2;
}
.pages>.row {
    padding:20px;
}

/* .pages .pageControl {
