    <script>
    
        $.ajax({
            url:'http://139.199.192.48:9090/api/getcategorytitle',
            success:function(d){
                var htmlStr = "";
                // console.log(d);
                for(var i=0;i<d.result.length;i++){
                    $.ajax({
                        url:"http://139.199.192.48:9090/api/getcategory",
                        data:{titleid:d.result[i].titleId},
                        _title:d.result[i].title,
                        success:function(d){
                            d.title = this._title;
                            // console.log(d);
                            htmlStr += template('list_tpl',d);
                            console.log(htmlStr);
                            // $('.main>ul').html(htmlStr);
                        }
                    });
                }
                console.log(htmlStr);
                console.log(htmlStr);
                // $('.main>ul').html(htmlStr);
            }
        });
        
        $('.main').on('click','.main_list',function(){
            // $(this).next().slideToggle();
            $('.son_list').css('display','none');
            $(this).next().toggle('ul_hide');
        });
    </script>
    <script type="text/template" id="list_tpl">
            <li >
               <div class="main_list">
