.view2 {
  font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;
  position: relative;
}
.view2 .discount_footer {
  margin-top: 50px;
  height: 53px;
  background-color: #f2f2f2;
  line-height: 53px;
  text-align: center;
  font-family: "微软雅黑";
  font-size: 16px;
}
.view2 .to_count {
  border-bottom: 1px solid #cccccc;
  border-top: 1px solid #cccccc;
  background-color: #f5f5f5;
}
.view2 .to_count .to_count_word {
  height: 35px;
  line-height: 35px;
}
.view2 .open_now {
  position: relative;
  height: 90px;
  width: 100%;
  background-color: #f2f2f2;
}
.view2 .open_now .buy_app {
  z-index: 10;
  height: 80px;
  width: 100%;
  background-color: #434343;
  padding: 7px;
  position: absolute;
  top: 18px;
  left: 8px;
}
.view2 .open_now .buy_app .buy_title {
  position: absolute;
  left: 75px;
  top: 12px;
  color: white;
}
.view2 .open_now .buy_app .buy_title p:nth-of-type(1) {
  font-size: 18px;
  float: none;
  overflow: hidden;
  font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;
}
.view2 .open_now .buy_app .buy_title p:nth-of-type(2) {
  color: #fee6c2;
  float: none;
  overflow: hidden;
  font-size: 14px;
  font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;
}
.view2 .open_now .buy_app .open_r_now {
  position: absolute;
  top: 14px;
  right: 7px;
}
.view2 .open_now .buy_app .open_r_now a {
  padding: 8px 12px;
  color: white;
  background-color: #6db110;
  border-radius: 4px;
}
.view2 .to_top {
  background-color: #efefef;
}
.view2 .to_top div {
  text-align: center;
  font-size: 16px;
  border-bottom: 1px solid #c4c4c4;
}
.view2 .to_top div a {
  color: black;
  height: 35px;
  line-height: 35px;
}
.view2 .to_top div:nth-of-type(1) {
  border-right: 1px solid #c4c4c4;
}
.view2 .to_top div:nth-of-type(2) {
  border-right: 1px solid #c4c4c4;
}
.view2 .blank {
  height: 60px;
  background-color: #efefef;
}
.view2 .back_buy {
  width: 100%;
  position: fixed;
  transform: translateZ(0);
  left: 0;
  bottom: 0px;
  z-index: 10;
  background-color: white;
  height: 40px;
}
.view2 .back_buy .commit_bot {
  height: 40px;
  text-align: center;
  line-height: 40px;
  font-size: 30px;
  display: inline-block;
}
.view2 .back_buy .back_go {
  height: 40px;
  line-height: 40px;
  text-align: center;
  background-color: #ff9000;
  color: white;
  display: inline-block;
}
.app_load {
  position: fixed;
  bottom: 20px;
  right: 20px;
  z-index: 20;
}
html,
body {
  height: 100%;
  overflow: auto;
  margin: 0;
}
