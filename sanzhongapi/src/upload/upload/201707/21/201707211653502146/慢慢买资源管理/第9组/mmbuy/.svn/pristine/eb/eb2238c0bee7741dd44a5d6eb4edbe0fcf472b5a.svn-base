* {
  padding: 0;
  margin: 0;
}
a {
  -webkit-tap-highlight-color: apprent;
  text-decoration: none;
}
@media (min-width: 1200px) {
  .center {
    width: 1000px;
    margin: 0 auto;
  }
}
.view .save_head {
  width: 100%;
  background-color: #ff841d;
}
.view .save_head .pull-left {
  font-size: 20px;
  line-height: 40px;
  color: white;
}
.view .save_head .save_title {
  line-height: 40px;
  height: 40px;
  display: inline-block;
  text-align: center;
  color: white;
  font-size: 24px;
  margin: 0 auto;
  width: 208px;
}
.view .save_head .pull-right {
  line-height: 40px;
}
.view .save_head .pull-right img {
  width: 28px;
  height: 28px;
}
.view .center .save_list {
  width: 100%;
}
.view .center .cloth {
  color: #333333;
  margin-top: 10px;
  position: relative;
}
.view .center .cloth img {
  width: 92px;
  height: 92px;
}
.view .center .cloth .cloth_word {
  position: absolute;
  height: 113px;
  left: 100px;
  top: 0;
}
.view .center .cloth .cloth_word .word_top {
  font-size: 15px;
}
.view .center .cloth .cloth_word .word_top span {
  color: #ff6c00;
}
.view .center .cloth .cloth_word .word_bot {
  position: absolute;
  bottom: 0;
  font-size: 12px;
  color: #a1a1a1;
}
.view .center .commits {
  font-family: "微软雅黑";
  text-align: right;
  font-size: 12px;
  color: #a1a1a1;
  margin-top: 10px;
}
.view .center .commits span {
  font-size: 12px;
}
.view .save_foot {
  line-height: 78px;
  height: 78px;
  background-color: #f2f2f2;
}
.view .save_foot .top_page {
  text-align: center;
}
.view .save_foot .top_page a {
  padding: 7px 20px;
  border: 1px solid #d2d2d2;
  background-color: #e1e1e1;
  border-radius: 4px;
  color: black;
}
.view .save_foot .middle_btn {
  text-align: center;
}
.view .save_foot .middle_btn select {
  width: 124px;
  height: 30px;
}
.view .to_save {
  border-bottom: 1px solid #cccccc;
  border-top: 1px solid #cccccc;
  background-color: #f5f5f5;
}
.view .to_save .to_save_word {
  height: 35px;
  line-height: 35px;
}
.view .to_top {
  background-color: #efefef;
}
.view .to_top div {
  text-align: center;
  font-size: 16px;
  border-bottom: 1px solid #c4c4c4;
}
.view .to_top div a {
  color: black;
  height: 35px;
  line-height: 35px;
}
.view .to_top div:nth-of-type(1) {
  border-right: 1px solid #c4c4c4;
}
.view .to_top div:nth-of-type(2) {
  border-right: 1px solid #c4c4c4;
}
.app_load {
  position: fixed;
  bottom: 24px;
  right: 20px;
}
