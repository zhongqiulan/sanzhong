@font-face {
  font-family: 'icomoon';
  src:  url('/fonts/icomoon.eot?xuq1cy');
  src:  url('/fonts/icomoon.eot?xuq1cy#iefix') format('embedded-opentype'),
    url('/fonts/icomoon.ttf?xuq1cy') format('truetype'),
    url('/fonts/icomoon.woff?xuq1cy') format('woff'),
    url('/fonts/icomoon.svg?xuq1cy#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
