.navbar {
  margin-bottom: 10px;
}
.navbar .container-fluid {
  background-color: #ff841d;
}
.navbar .container-fluid .header-logo a.navbar-brand img {
  height: 25px;
}
.navbar .container-fluid .app-download img {
  height: 34px;
  margin: 8px;
}
.container {
  margin-bottom: 10px;
}
.container .col-md-12 input.form-control {
  border-top-left-radius: 17px;
  border-bottom-left-radius: 17px;
  background-color: #f1f1f1;
}
.container .col-md-12 span.input-group-addon {
  border-top-right-radius: 17px;
  border-bottom-right-radius: 17px;
  background-color: #ff841d;
  color: white;
}