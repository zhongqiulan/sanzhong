<script type="text/template" id="menu_tpl">
    {{each result as v }}
    <div class="col-xs-3 ">
                <div class="thumbnail">
                    <a href='{{v.titlehref}}'>
                        
                          {{#v.img}}
                        <div class="caption">
                            
                             {{v.name}}   
                        </div>
                    </a>
                </div>
    </div>
    {{/each}}
</script>

<script type="text/template" id="recom_tpl">
      {{each result as v }}
    <li class="clearfix">
                    <a href='javascript:;'>
                        <div class="brand_Img  ">
                           {{#v.productImg2}}
                        </div>
                        <div class="img_text clearfix ">
                            <div class="title">
                                {{v.productName}}<span>{{v.productPinkage}}</span>
                            </div>
                            <div class="other clearfix">
                                <span class="mall pull-left ">{{v.productFrom}} | {{v.productTime}}</span>
                                <span class="comment pull-right ">
                                 <i class="icon-comment"></i>{{v.productComCount}}
                                </span>
                            </div>
                        </div>
                    </a>
                </li>
        {{/each}}
</script>