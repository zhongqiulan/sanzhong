*{
    list-style: none;
    margin:0;
    padding:0;
}
// 顶部导航条
.navbar{
    margin-bottom:10px;
    .container-fluid{
        background-color: #ff841d;
        .header-logo{
            a.navbar-brand{
                img{
                    height: 25px;
                }
            }
        }
        .app-download{
            img{
                height: 34px;
                margin:8px;
            }
        }
    }
}
//搜索框
.container{
    margin-bottom:10px;
    .col-md-12{
        div.input-group{
            width:100%;
        }
        input.form-control{
            border-radius:17px;
            background-color: #f1f1f1;
            width:110%;
        }
        span.input-group-addon{
            position:relative;
            border-radius:17px; 
            background-color: #ff841d;  
            color:white;   
            width:60px;
            z-index: 2;
        }
    }
}
//main内容
.ul_hide{
    display:none;
}
.ul_show{
    display:block;
}
.main {
    background-color: #f2f2f2;
    padding:5px;
    height: 500px;
    >ul{
        border:1px solid #d3d3d3;
        border-radius:5px;
        overflow:hidden;
        >li{
            >div.main_list{
                background-color: yellow;
                height:40px;
                background-color: #ebebeb;
                border-bottom:1px solid #ccc;
                // &:after{
                //     content:'';
                //     display:block;
                //     clear:both;
                //     height:0;
                //     visibility:hidden;

                // }
                // .clearfix{zoom:1}
                span{
                    height: 40px;
                    line-height: 40px;
                    padding:0 15px;
                    font-size:16px;
                }
                span:first-of-type{
                    float:left;
                    font-weight:bold;
                }
                span:last-of-type{
                    float:right;
                    color:#919191;
                }
            }
            >ul{
                // clear:both;
                
                &:after{
                    content:'';
                    display:block;
                    clear:both;
                    height:0;
                    visibility:hidden;

                }
               >li{
                   height: 30px;
                   width:33.3%;
                   float:left;
                   border:1px solid #ccc;
                   background-color: #FFF;
                   line-height: 30px;
                   text-align: center;
                   >a{
                    color:#343434;
                   }
               }
            }
        }
    }
}
//底部区域
footer{
    overflow:hidden;
    .navbar{
        height:100px;
        margin:0;
        background-color: #efefef;
        .row{
            div{
                border-top:1px solid #cbcbcb;
                border-bottom:1px solid #cbcbcb;
                height: 35px;
                line-height: 35px;
                text-align: center;
                a {
                    color:black;
                    font-size:14px;
                        span{
                            color:#747474;
                            font-size:12px;
                        }
                    }
            }
            div:nth-of-type(2){
                border-left:1px solid #cbcbcb;
                border-right:1px solid #cbcbcb;
            }
        }
        div.app-updown{
            text-align: center;
            padding-top:5px;
            font-size:12px;
            a{
                color:red;
            }
            span:first-of-type{
                color:black;
            }
            span:last-of-type{
                color:#7b7b7b;
            }
            p{
                color:#7b7b7b;
            }
        }
    }
}