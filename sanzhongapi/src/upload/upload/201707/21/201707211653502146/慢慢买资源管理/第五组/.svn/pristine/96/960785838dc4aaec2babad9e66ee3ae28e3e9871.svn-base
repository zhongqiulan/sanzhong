            </a>
        </li>
        {{/each}}
    </ul>


</script>
<script id="product_list" type="text/html">
    <ul>
        {{each result as value i}}
        <li>
            <a href="#">
                {{#value.productImg2}}
                <span>
                        <p>{{value.productName}}</p>
                        <p>{{value.productPinkage}}</p>
                        <p>{{value.productFrom}}</p>
                        <p>{{value.productTime}}</p>
                        <span class="dsb">
                            <span class="glyphicon-random glyphicon glyphicon-comment"></span>
                            <p>{{value.productComCount}}</p>
                        </span>
                    </span>
            </a>
        </li>
        {{/each}}
    </ul>
</script>

