    <script type="text/template" id="temp_product_list">
        <ul class="nav nav-tabs reorder">
            <li role="presentation" class="active"><a href="#home" class="on" aria-controls="home" role="tab" data-toggle="tab">综合</a></li>
            <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">销量</a></li>
            <li role="presentation"><a href="#messages" aria-controls="messages" role="tab" data-toggle="tab">价格</a></li>
            <li role="presentation"><a href="#settings" aria-controls="settings" role="tab" data-toggle="tab">筛选</a></li>
        </ul>
        <div class="tab-content product-list">
            <div role="tabpanel" class="tab-pane active syn" id="home">
                <ul>
                    {{each result as v i}}
                    <li class="media">
                        <a href="./commentlist.html?id={{v.productId}}">
                            <div class="media-left pic">
                                {{#v.productImg}}
                            </div>
                            <div class="media-body info">
                                <h4 class="media-heading">{{v.productName}}</h4>
                                <div class="price">
                                    <em>{{v.productPrice}}</em>
                                </div>
                                <div class="other">
                                    <span>{{v.productQuote}}</span>
                                    <span class="com">{{v.productCom}}</span>
                                </div>
                            </div>
                        </a>
                    </li>
                    {{/each}}
                </ul>
            </div>
            <div role="tabpanel" class="tab-pane" id="profile"></div>
            <div role="tabpanel" class="tab-pane" id="messages"></div>
            <div role="tabpanel" class="tab-pane" id="settings"></div>
        </div>
        <!-- 分页 -->
        <div class="page">
            <ul class="clearfloat">
                <li><a href="javascript:;" id="prev" class="previous">上一页</a></li>
                <li>
                    <select class="selectPage" id="selectPage" data-index="1" data-pages="{{pages}}">
                        <option value="1">1/{{pages}}</option>
                        <option value="2">2/{{pages}}</option>
                        <option value="3">3/{{pages}}</option>
                    </select>
                </li>
                <li><a href="javascript:;" id="next" class="next">下一页</a></li>
            </ul>
        </div>
    </script>


    <script src="../../lib/jquery/jquery.min.js"></script>
    <script src="../../lib/bootstrap/js/bootstrap.min.js"></script>
