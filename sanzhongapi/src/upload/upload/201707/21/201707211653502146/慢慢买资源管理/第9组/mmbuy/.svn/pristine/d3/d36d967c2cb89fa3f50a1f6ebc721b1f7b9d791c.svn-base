//底部
 .foot_bar{
        width: 100%;
        height: 34px;
        line-height: 34px;
        margin-top: 10px;
        text-align: center;
        ul{
            li{
                float: left;
                width: 33.33%;
                border-top: 1px solid #ccc;
                border-bottom: 1px solid #ccc;
                &+li{
                    border-left: 1px solid #ccc;
                }
                .icon-top{
                    font-size: 15px;
                }
            }
        }
    }
    footer{
        width: 100%;
        text-align: center;
        color:#5a5a5a;
        font-size: 12px;
        padding: 15px 0;
        .foot_top{
            a{
                display: inline-block;
            }
            a:nth-of-type(1){
                color:red;
            }
        }
    }

