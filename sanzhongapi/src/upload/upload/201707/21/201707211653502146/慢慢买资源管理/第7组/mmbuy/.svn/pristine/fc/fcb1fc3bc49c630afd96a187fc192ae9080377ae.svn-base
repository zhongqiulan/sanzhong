}
.view main .category > ul {
  border: 1px solid #ccc;
  margin: 5px;
  border-radius: 5px;
}
.view main .category > ul > li {
  background-color: #ebebeb;
  width: 100%;
  padding: 0 10px;
  position: relative;
  border-bottom: 1px solid #808080;
}
.view main .category > ul > li .title a {
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  text-decoration: none;
  font-weight: bold;
}
.view main .category > ul > li .title i {
  position: absolute;
  width: 60px;
  height: 40px;
  padding: 12px 18px;
  top: 0;
  right: 0;
  font-size: 16px;
}
.view main .category > ul > li .ct_info ul {
  background-color: #f9f9f9;
  border-bottom: 1px solid #808080;
  margin: 0 -10px;
}
.view main .category > ul > li .ct_info ul li {
  height: 40px;
  padding: 0;
  border-bottom: 1px solid #ccc;
  border-right: 1px solid #ccc;
}
.view main .category > ul > li .ct_info ul li:nth-of-type(3n) {
  border-right: none;
}
.view main .category > ul > li .ct_info ul li a {
  line-height: 40px;
  width: 100%;
  text-align: center;
  text-decoration: none;
}
