@import "../base.less";

.views{
    header{
        width: 100%;
        height: 50px;
        background-color: #ff841d;
        text-align: center;
        position: relative;
        .back{
            .icon-back{
                color: #fff;
                font-size: 20px;
                line-height: 50px;
                padding-left: 10px;
            }
        }
        h3{
            position: absolute;
            color: #fff;
            line-height: 50px;
            left: 50%;
            transform: translate(-50%);
        }
        .app-download{
            padding:7px;
            img{
                max-height:34px;
            }
        }
    }
    .wrap_container{
        width: 100%;
        padding: 30px 40px;
        .brand_list{
            li{
                width: 33.33%;
                height: 40px;
                padding: 2px;
                text-align: center;
                a{
                    height: 34px;
                    line-height: 34px;
                    border: 1px solid #ccc;
                    background-color: #f4f4f4;
                }
                
            }
        }
    }
    .bread{
        height: 40px;
        line-height: 40px;
        background-color: #ebebeb;
        border: 1px solid #ccc;
        padding-left: 20px;
    }
}
