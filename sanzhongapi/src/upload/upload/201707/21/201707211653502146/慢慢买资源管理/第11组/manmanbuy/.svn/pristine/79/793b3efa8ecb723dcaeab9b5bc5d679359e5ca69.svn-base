@import '../base/base.less'; 
 html,body{
     height:100%;
 }
.beascss(){
    em{
         font-style: normal;
    }
    b{
        font-weight: normal;
    }
    i{
      font-style: normal;  
    }
    font-size:12px;
    ul{
        margin: 0;
        padding: 0;
    }
    a{
        display: block;
        color: #666;
    }
    input{
        border: none;
    }
}
//顶部样式和搜索框样式
.top(){
   .top{
        .navbar{
            margin: 0;
        }
        .navbar-default{
            width: 100%;
            height: 50px;
            background: #ff841d;
            border-bottom: 1px solid #e6e6e6;           
            .navbar-brand{
                width: 100%;
                img{
                    width: 185px;
                }
            }
            .pull-right{
                @media screen and (max-width: 300px){
                    display: none;
                }
                width:34px;
                padding-top:8px;
                position:relative;
                img{
                    width:34px;
                    position:absolute;
                    right:0;
                    top:6px;
                }
            }
        }
    }
     .search{
            border-bottom: 1px solid #e6e6e6;
            height: 50px;
            padding: 10px;
            background-color: #fff;
            position:relative; 
            input{
				border:none;
                outline: none;
                padding: 5px;
                height: 30px;
                background: #e6e6e6;
                border-radius: 25px;               
                width: 100%;
            }
            button{
				border:none;
                height: 30px;
                background: #ff841d;
                 border-radius: 25px;
                 color:#fff;
                 width: 60px;
                  position:absolute;
                  top:10px;
                  right:10px;

            }
    }
}


//底部的样式
.footer(){
             
            footer{
            background: #efefef;
            .menu{
                ul{
                    padding:0;
                    margin:0;
                }
                li{
                    height: 35px;
                    line-height: 35px;
                    text-align: center;
                    border-bottom: 1px solid #c4c4c4;
                    border-top: 1px solid #c4c4c4;
                    float: left;
                    width: 33.3%;
                    &:nth-of-type(2){
                        border:1px solid #c4c4c4;
                    }
                    a{
                        display:block; 
                        color: #000;
                    }
                    img{
                        width: 15px;
                        display: inline-block;
                        margin-bottom: 6px;
                        margin-right: 3px;
                    }
                }
            }
            .xinxi{
                font-size: 13px;
                text-align: center;
                padding:15px 0;
                p{
                    margin: 0;
                    color:#747474;
                    a{
                        display: inline-block;
                         &:nth-of-type(1){
                             color: red;
                         }
                         &:nth-of-type(2){
                             color: #000;
                         }
                    }
                }
            }
    }

}


