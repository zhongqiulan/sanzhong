.pages {
    width: 100%;
    text-align: center;
    /*margin-top: 20px;*/
    /*margin-bottom: 20px;*/
    padding:20px 0;
    background-color: rgb(242, 242,242)
}


table {
    border-collapse: collapse;
    border-spacing: 0;
    width: 100%;
}


.pages button {
    outline: none;
    border: none;
    display:inline-block;
    box-sizing: border-box;
    background-color: #5d0707;
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#f1f1f1), to(#d4d4d4));
    border-bottom: 1px solid #bababa;
    box-shadow: 0 0 1px #666;
    border-radius: 5px;
    text-align: center;
    font-size: 14px;
    padding: 5px 20px;
    color: #000;
}
 .footer();//底部的样式
}
// 侧边栏
 .sx-aside{
        left:-80%;
        -webkit-transition: left 0.6s;
        width:80%;
        position:fixed;
        top:0;
        // left:0;
        z-index:12;
        background: #fff;
        height:100%;
        overflow-y:scroll;
        .title{
            text-align:center;
            border-bottom:1px solid #ccc;
            h1{
                margin:0;
                font-size:20px;
                height:44px;
                line-height: 44px;
                width:100%;
                padding-right:40px;
                position:relative;
                i{
                    position:absolute;
                    right:10px;
                    top:10px;
                    font-size:20px;                   
                }
            }
        }

        .group{

            .row{
                padding:0 15px;
                margin-right:0px;
                margin-left:0px;
                .rowhead{
                  text-align: left;
                  padding:15px 0px;
                  span{
                      &:nth-of-type(1){
                          font-size:14px;
                      }
                       &:nth-of-type(2){
                           color:#ff5000;
                       }
                        &:nth-of-type(3){
                            float:right;
                        }
                  }
                }
                .rowbody{
                    max-height:70px;
                    overflow: hidden;
                    ul{
                        li{
                            a{
                                width:30%;
                                height:28px;
                                line-height: 28px;
                                float:left; 
                                background: #f2f2f2;
                                border-radius:5px;
                                margin-left:2%;
                                margin-bottom:8px;
                                color:#aaa;
                            }
                            .bgc_orange{
                                 background: #ff5000;
                                 }                                                    
                        }
                    }
                }
            }
        }
    }

// 遮罩层的样式
.master{
    display: none;
        position:fixed;
        width:100%;
