        z-index:10;
    }

    






//详情页的样式
.cate-list{
    .beascss();
    .top();
   .tab-content{
       #home{
           img{
               width: 20px;
           }
       }
   }
    
    .cpzs{     
         border-bottom: 1px solid #ccc;
        // 条状导航
        .cpzs-nav{
            height: 38px;
            background: linear-gradient(to bottom,#fff, #ebebeb);
            line-height: 38px;
            font-size: 14px;
            border-bottom: 1px solid #ccc;
            ul{
                padding-left: 10px;
                li{
                    color: #a1a1a1;
                    float: left;
                }
            }
        }
        //产品内容
        .cp-content{
            padding: 10px;
            .pic{
                height: 180px;
                text-align:center;
                position:relative;
                .spimg{
                width: 160px;height: 160px;
                margin: 0 auto;
                }
                .scbtn{
                    width: 39px;
                    height: 39px;
                    display: block;
                    border-radius: 50%;
                    position: absolute;
                    top: 0;
                    right: 0;

                    }
                }
            .text-ms{
                font-size: 15px;
            }
            .other{
                color: #a1a1a1;
                padding-top: 10px;
            }
           
        }
    }
    .cp-tab{
        padding-top: 20px;
        background: #f2f2f2;
        //tap栏的样式
        .nav-tabs{
            font-size: 15px;
            background: linear-gradient(to bottom,#ffd050, #ffbb00);
            border: solid 1px #c14d00;
            border-left: none;
            margin-bottom: 6px;
            .active{          
                    a{   
                    margin: 0;
                    font-size: 15px;
                    background:  linear-gradient(to bottom,#ffa632, #ff9000);
                    }
                    .caret{
                     display: block;
                    }
                }
            li{
                margin: 0;
                padding: 0;
                text-align: center;
                border-left:solid 1px #c14d00; 
                position: relative;
                .caret{
                    position: absolute;
                    color: #ff9000;
                    bottom:0;
                    left:50%;
                    border-width: 5px;
                    transform: translate(-50%,100%);
                    display: none;
                }
                &:active{
                 a{
                    background:  linear-gradient(to bottom,#ffa632, #ff9000);  
                 }   
                }
                        
                a{
                    padding: 10px 0;
                    color: #fff;
                    border: none;
                }
            }
        }
        // 产品内容部分
        .tab-content{
            //比较购买对应的模块样式
           
            padding-bottom: 10px;
            // padding-top: 10px;
            background: #f2f2f2;
             .jtcp{
                 font-size: 14px;
                 position: relative;
                 padding: 12px;
                 color:#5a5a5a;
                .logo{
                    display: inline-block;                   
                    width: 20px;
                    height: 20px;
                    margin-right: 5px;
                }
                .right-jg{
                    position: absolute;
                    top: 10px;
                    right: 10px;
                    color: #c14d00;
                }
                .cpzs-nav{
                    li{
                         a{
                            color:#999;
                            font-size: 15px;
                        }
                    }
                    
                }
             }

             .evaluate{
                font-size: 14px;
                text-align: center;          
                 .pjfl{
                    height: 31px;
                    line-height: 31px;
                    ul{
                        li{
                            float: left;
                            a{
                                color: #5a5a5a;
                            }
                            .active{
                                margin-top: -1px;
                                border-bottom: 1px solid #ff9000;
                            }
                            border-bottom: 1px solid #ccc;
                        }
                    }
                    
                 }

                 .jzgd{
                        height: 40px;
                        width: 100%;
                        background: #e6e6e6;
                        line-height: 40px;
                    }

                 .pj-content{
                     padding: 10px;
                     padding-bottom: 10px;
                      border-bottom: 1px solid #ccc;
                     .user-info{
                        //  height: 36px;
                         line-height:12px;
                         margin: 0;
                         padding: 0;                      
                          .user-name{
                            text-align: left;
                            .date{
                                text-align: right;
                                font-size: 12px;
                            }
                            
                         }
                         .pj-data{
                             margin-top: -20px;
                             text-align: left;
                             position: relative;
                             top: -8px;
                             .star{
                                 color: #ffd050;
                             }
                             .shangcheng{
                                 font-size: 12px;
                                 text-align: right;
                             }
                         }
                     }
                     .content{
                            position: relative;
                             padding-bottom: 20px;
                             margin-bottom: 20px;
                             text-align: left;
                            .huif{
                                position:absolute;
                                bottom: 0;
                                left: 0px;
                                color:#999;
                                font-size: 12px;
                            }

                     }
                     
                 }
             }
        }
    }

    .footer();
}