.all {
  background-color: #ebebeb;
}
.home_nav {
  height: 36px;
  line-height: 36px;
  padding-left: 10px;
  border-top: solid 1px #ebebeb;
  border-bottom: 1px solid #ccc;
}
.home_nav a {
  font-size: 14px;
  color: #5a5a5a;
}
.brand_ranking .ranking_head {
  height: 38px;
  line-height: 38px;
  padding-left: 10px;
  border: 1px solid #ccc;
  font-size: 16px;
  margin: 10px 5px;
  color: #874604;
}
.brand_introduce {
  height: 40px;
  line-height: 40px;
  padding-left: 10px;
  border-bottom: 1px solid #ccc;
  font-size: 16px;
  margin: 10px 5px;
  position: relative;
}
.brand_introduce a {
  color: #000;
  font-family: '微软雅黑';
  font-weight: bold;
}
.brand_introduce span {
  position: absolute;
  right: 20px;
  top: 50%;
  margin-top: -9.5px;
  color: '#aaa';
}
