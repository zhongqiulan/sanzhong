.sx-aside {
  left: -80%;
  -webkit-transition: left 0.6s;
  width: 80%;
  position: fixed;
  top: 0;
  z-index: 12;
  background: #fff;
  height: 100%;
  overflow-y: scroll;
}
.sx-aside .title {
  text-align: center;
  border-bottom: 1px solid #ccc;
}
.sx-aside .title h1 {
  margin: 0;
  font-size: 20px;
  height: 44px;
  line-height: 44px;
  width: 100%;
  padding-right: 40px;
  position: relative;
}
.sx-aside .title h1 i {
  position: absolute;
  right: 10px;
  top: 10px;
  font-size: 20px;
}
.sx-aside .group .row {
  padding: 0 15px;
  margin-right: 0px;
  margin-left: 0px;
}
.sx-aside .group .row .rowhead {
  text-align: left;
  padding: 15px 0px;
}
.sx-aside .group .row .rowhead span:nth-of-type(1) {
  font-size: 14px;
}
.sx-aside .group .row .rowhead span:nth-of-type(2) {
  color: #ff5000;
}
.sx-aside .group .row .rowhead span:nth-of-type(3) {
  float: right;
}
.sx-aside .group .row .rowbody {
  max-height: 70px;
  overflow: hidden;
}
.sx-aside .group .row .rowbody ul li a {
  width: 30%;
  height: 28px;
  line-height: 28px;
  float: left;
  background: #f2f2f2;
  border-radius: 5px;
  margin-left: 2%;
  margin-bottom: 8px;
  color: #aaa;
}
.sx-aside .group .row .rowbody ul li .bgc_orange {
  background: #ff5000;
}
