html,
body {
  width: 100%;
  box-sizing: border-box;
  padding: 0;
  margin: 0;
  font-family: "Hiragino Sans GB", "Microsoft YaHei", "微软雅黑", tahoma, arial, simsun, "宋体";
  background-color: #ffffff;
}
img {
  display: block;
}
a {
  display: inline-block;
  -webkit-tap-highlight-color: none;
}
a:hover,
a:focus {
  text-decoration: none;
}
ul {
  list-style: none;
}
html,
body {
  height: 100%;
}
.cate-lest .top .navbar {
  margin: 0;
}
.cate-lest .top .navbar-default {
  width: 100%;
  height: 50px;
  background: #ff841d;
  border-bottom: 1px solid #e6e6e6;
}
.cate-lest .top .navbar-default .navbar-brand {
  width: 100%;
}
.cate-lest .top .navbar-default .navbar-brand img {
  width: 185px;
}
.cate-lest .top .navbar-default .pull-right {
  width: 34px;
  padding-top: 8px;
  position: relative;
}
@media screen and (max-width: 300px) {
  .cate-lest .top .navbar-default .pull-right {
    display: none;
  }
}
.cate-lest .top .navbar-default .pull-right img {
  width: 34px;
  position: absolute;
  right: 0;
  top: 6px;
}
.cate-lest .search {
  border-bottom: 1px solid #e6e6e6;
  height: 50px;
  padding: 10px;
  background-color: #fff;
  position: relative;
}
.cate-lest .search input {
  border: none;
  outline: none;
  padding: 5px;
  height: 30px;
  background: #e6e6e6;
  border-radius: 25px;
  width: 100%;
}
.cate-lest .search button {
  border: none;
  height: 30px;
  background: #ff841d;
  border-radius: 25px;
  color: #fff;
  width: 60px;
  position: absolute;
  top: 10px;
  right: 10px;
}
.cate-lest a {
  color: #000;
}
.cate-lest .Commodity-list {
  padding: 5px;
  font-size: 16px;
  background: #f2f2f2;
}
.cate-lest .Commodity-list .container {
  padding: 0;
}
.cate-lest .Commodity-list .List-item {
  border: 1px solid #e6e6e6;
  border-radius: 5px;
}
.cate-lest .Commodity-list .List-item .btn {
  padding-top: 10px;
  width: 100%;
  text-align: left;
  height: 40px;
  font-weight: 700;
}
.cate-lest .Commodity-list .List-item .col {
  width: 100%;
  padding: 0;
  background: #fff;
  border-left: 1px solid #ccc;
}
.cate-lest .Commodity-list .List-item .col li {
  border-bottom: 1px solid #ccc;
  border-right: 1px solid #ccc;
  background: #fff;
  height: 40px;
  font-size: 13px ;
  line-height: 40px;
  text-align: center;
  padding: 0;
  margin: 0;
}
.cate-lest footer {
  background: #efefef;
}
.cate-lest footer .menu ul {
  padding: 0;
  margin: 0;
}
.cate-lest footer .menu li {
  height: 35px;
  line-height: 35px;
  text-align: center;
  border-bottom: 1px solid #c4c4c4;
  border-top: 1px solid #c4c4c4;
  float: left;
  width: 33.3%;
}
.cate-lest footer .menu li:nth-of-type(2) {
  border: 1px solid #c4c4c4;
}
.cate-lest footer .menu li a {
  display: block;
  color: #000;
}
.cate-lest footer .menu li img {
  width: 15px;
  display: inline-block;
  margin-bottom: 6px;
  margin-right: 3px;
}
.cate-lest footer .xinxi {
  font-size: 13px;
  text-align: center;
  padding: 15px 0;
}
.cate-lest footer .xinxi p {
  margin: 0;
  color: #747474;
}
.cate-lest footer .xinxi p a {
  display: inline-block;
}
.cate-lest footer .xinxi p a:nth-of-type(1) {
  color: red;
}
.cate-lest footer .xinxi p a:nth-of-type(2) {
  color: #000;
}
