header {
  width: 100%;
}
header h1 {
  width: 100%;
  border-bottom: 1px solid  #e6e6e6 ;
  background-color: #ff841d;
  height: 50px;
  padding-top: 13px;
  position: relative;
  padding-left: 25px;
}
header h1 a {
  display: block;
  height: 100%;
  width: 100%;
  background: url('../../../../images/header_logo.png') no-repeat 0 0;
  background-size: 185.5px ;
}
header h1 span {
  height: 34px;
  width: 34px;
  position: absolute;
  background: url('../../../../images/header_app.png') no-repeat 0 0;
  background-size: 34px ;
  top: 10px;
  right: 10px;
}
header .zyzsreach .input-group {
  width: 100%;
  padding: 10px;
  position: relative;
  background-color: #fff;
}
header .zyzsreach .input-group input {
  border-radius: 50px;
  width: 94%;
  font-size: 16px;
  background-color: #f6f6f6;
  outline-color: none;
  border-color: none;
  font-weight: 600;
  border: none;
  outline: none;
}
header .zyzsreach .input-group span {
  width: 63px;
  border-radius: 30px;
  position: absolute;
  top: 10px;
  right: 10px;
  z-index: 999;
  height: 34px;
  background-color: #ff841d;
  font-size: 16px;
  color: #fff;
  line-height: 19px;
}
.criticism_all{
  .criticism_top{
    padding: 10px 10px;
    background-color:#dbdbdb;
    text-align: center;
    font-weight: bold;
 }
  .criticism_buttom{
      position: relative;
      .time{
          position: absolute;
          right:0;
      }
  }  
}

