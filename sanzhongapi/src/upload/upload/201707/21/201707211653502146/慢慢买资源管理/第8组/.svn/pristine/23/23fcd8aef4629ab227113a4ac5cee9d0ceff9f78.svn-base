// 顶部导航条
.navbar{
    margin-bottom:10px;
    .container-fluid{
        background-color: #ff841d;
        .header-logo{
            a.navbar-brand{
                img{
                    height: 25px;
                }
            }
        }
        .app-download{
            img{
                height: 34px;
                margin:8px;
            }
        }
    }
}
//搜索框
.container{
    margin-bottom:10px;
    .col-md-12{
        input.form-control{
            border-top-left-radius:17px;
            border-bottom-left-radius:17px;
            background-color: #f1f1f1;
        }
        span.input-group-addon{
            border-top-right-radius:17px;
            border-bottom-right-radius:17px;  
            background-color: #ff841d;  
            color:white;   
        }
    }
}
//main内容
.main {
    height: 600px;
    background-color:#ffccff;
}
//底部区域
footer{
    overflow:hidden;
    .navbar{
        height:100px;
        margin:0;
        background-color: #efefef;
        .row{
            div{
                border-top:1px solid #cbcbcb;
                border-bottom:1px solid #cbcbcb;
                height: 35px;
                line-height: 35px;
                text-align: center;
                a {
                    color:black;
                    font-size:14px;
                        span{
                            color:#747474;
                            font-size:12px;
                        }
                    }
            }
            div:nth-of-type(2){
                border-left:1px solid #cbcbcb;
                border-right:1px solid #cbcbcb;
            }
        }
        div.app-updown{
            text-align: center;
            padding-top:5px;
            font-size:12px;
            a{
                color:red;
            }
            span:first-of-type{
                color:black;
            }
            span:last-of-type{
                color:#7b7b7b;
            }
            p{
                color:#7b7b7b;
            }
        }
    }
}