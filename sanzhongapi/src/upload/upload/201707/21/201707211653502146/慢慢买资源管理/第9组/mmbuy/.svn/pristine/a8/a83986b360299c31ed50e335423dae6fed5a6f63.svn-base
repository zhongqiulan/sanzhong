.navbar {
  height: 40px;
  margin-bottom: 0;
  background-color: #f4483e;
  position: relative;
}
.navbar .navbar-header .navbar-brand {
  width: 40px;
  height: 40px;
  color: #fff;
}
.navbar .navbar-center {
  font-size: 26px;
  position: absolute;
  left: 40%;
  top: 5px;
}
.navbar .navbar-center span {
  height: 40px;
  color: #fff;
}
.navbar .app-donwload {
  margin-top: 5px;
}
.navbar .app-donwload img {
  width: 40px;
  height: 40px;
}
.dealcre {
  color: #999;
  text-align: center;
}
.coupon_list a {
  color: #000;
  line-height: center;
  border-bottom: 1px solid #ccc;
  text-decoration: none;
}
.coupon_list a img {
  width: 100px;
  height: 80px;
}
.coupon_list a h4 {
  margin: 5px 0;
}
.coupon_list a p {
  margin: 5px 0;
  color: #f5b26f;
}
.coupon_list a .media-right span {
  margin-top: 25px;
  font-size: 20px;
  color: #999;
}
.coupon_list a .media-right span :before {
  height: 40px;
}
