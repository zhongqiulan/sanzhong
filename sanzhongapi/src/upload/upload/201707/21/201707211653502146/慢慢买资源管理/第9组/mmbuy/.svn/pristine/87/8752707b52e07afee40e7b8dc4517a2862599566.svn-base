* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
a {
  text-decoration: none;
  color: inherit;
  display: block;
  -webkit-tap-highlight-color: transparent;
}
a:focus,
a:active,
a:hover,
a:visited {
  color: #5a5a5a;
  text-decoration: none;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
}
input {
  outline: none;
  border: none;
}
ul,
ol {
  list-style: none;
}
.views header {
  width: 100%;
  height: 50px;
  background-color: #ff841d;
  text-align: center;
  position: relative;
}
.views header .back .icon-back {
  color: #fff;
  font-size: 20px;
  line-height: 50px;
  padding-left: 10px;
}
.views header h3 {
  position: absolute;
  color: #fff;
  line-height: 50px;
  left: 50%;
  transform: translate(-50%);
}
.views header .app-download {
  padding: 7px;
}
.views header .app-download img {
  max-height: 34px;
}
.views .wrap_container {
  width: 100%;
  padding: 30px 40px;
}
.views .wrap_container .brand_list li {
  width: 33.33%;
  height: 40px;
  padding: 2px;
  text-align: center;
}
.views .wrap_container .brand_list li a {
  height: 34px;
  line-height: 34px;
  border: 1px solid #ccc;
  background-color: #f4f4f4;
}
.views .bread {
  height: 40px;
  line-height: 40px;
  background-color: #ebebeb;
  border: 1px solid #ccc;
  padding-left: 20px;
}
