<body>

    <div class="views">
        <header class="clearfix">
            <div class="logo pull-left">
                <a href="javascript:;" class="header">
                <img src="../../images/header_logo.png" alt="">
            </a>
            </div>
            <div class="app-download pull-right">
                <a href="javascript:;">
                <img src="../../images/header_app.png" alt="">
            </a>
            </div>

        </header>
        <div class="searchBox">
            <input type="text" placeholder="请输入你想比价的产品" aria-describedby="basic-addon2">
            <span class="search_btn">搜索</span>
        </div>

        <!-- 主体内容 -->
        <div class="main">
            <!-- 导航标题 -->
            <div class="nav">
                <a href="../../index.html">首页</a> >
                <a href="./brandlist.html">品牌大全</a>
            </div>
            <!-- 品牌列表 -->
            <div class="model">

            </div>
            <!-- 品牌销量排行列表 -->
            <div class="product">

            </div>

            <!--登录注册-->
            <div class="foot_bar">
                <ul class="clearfix">
                    <li><a href="login.html">登录</a></li>
                    <li><a href="javascript:;">注册</a></li>
                    <li id="top"><a href="javascript:;"><i class="icon-top"></i>返回顶部</a></li>
                </ul>
            </div>
            <footer>
                <div class="foot_top">
                    <a href="javascript:;">手机APP下载</a>
                    <a href="index.html">慢慢买手机版</a> -- 掌上比价平台
                </div>
                <div class="foot_bottom">m.manmanbuy.com</div>
            </footer>
        </div>
    </div>




    <!-- 品牌列表 -->
    <script type="text/template" id="temp_brand_list">
        <div class="model-title">
            <h3>平板电视哪个品牌好</h3>
        </div>
        <div class="model-list">
            <ul>
                {{each result as v i}}
                <li>
                    <a href="./productlist.html?id={{v.brandTitleId}}">
                    {{if i>2}}
                    <em>{{i+1}}</em>
                    {{else}}
                    <em class="top{{i+1}}">{{i+1}}</em>
                    {{/if}}
                    <div class="inner">
                        <div class="tit">{{v.brandName}}</div>
                        <p>{{v.brandInfo}}</p>
                    </div>
                </a>
                </li>
                {{/each}}
            </ul>
        </div>
    </script>

    <!-- 品牌销量排行列表 -->
    <script type="text/template" id="temp_brand_product">
        <div class="product-title media-middle">
            <h3><a href="./productlist.html">平板电视产品销量排行</a></h3>
        </div>

        <ul class="product-list">
            {{each result as v i}} {{if i
            <4}} <li class="media">
                <div class="media-left pic">
                    {{#v.productImg}}
                </div>
                <div class="media-body info">
                    <h4 class="media-heading">{{v.productName}}</h4>
                    <div class="price">
                        <em>{{v.productPrice}}</em>
                        <span></span>
                    </div>
                    <div class="other">
                        <span>{{v.productQuote}}</span>
                        <span>全网{{v.productCom}}</span>
                    </div>
                </div>
                </li>
                {{/if}} {{/each}}
        </ul>
    </script>

