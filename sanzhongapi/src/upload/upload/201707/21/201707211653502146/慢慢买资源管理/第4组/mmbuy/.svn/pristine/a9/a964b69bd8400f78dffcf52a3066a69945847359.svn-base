.layout {
    #header {
        background-color: #ff841d;
        width: 100%;
        >h1 {
            height: 40px;
            line-height: 40px;
            text-align: center;
            font-size: 20px;
            font-weight: bold;
            color: #fff;
            margin: 0px 52px;
            padding: 0;
            overflow: hidden;
        }
        >.icon-back {
            position: absolute;
            ;
            top: 0;
            left: 5px;
            text-align: left;
            width: auto;
            line-height: 40px;
            overflow: hidden;
            a {
                color: #fff;
                font-size: 14px;
                display: block;
            }
        }
        >.icon-app {
            position: absolute;
            ;
            top: 0;
            right: 10px;
            text-align: right;
            width: auto;
            font-size: 14px;
            line-height: 40px;
            overflow: hidden;
            a {
                display: block;
                img {
                    width: 50%;
                }
            }
        }
    }
    #discount {
        border-top: 1px solid #e0e0e0;
        margin-top: 10px;
        >.media {
            // >a {
            //     display: block;
            //     position: absolute;
            //     top: 0px;
            //     left: 0px;
            //     width: 100%;
            //     height: 100%;
            // }
            cursor: pointer;
            padding: 5px;
            border-bottom: 1px solid #e0e0e0;
            margin-top: 0;
            position: relative;
            >.media-left {
                >.rank {
                    display: block;
                    width: 40px;
                    height: 20px;
                    line-height: 20px;
                    position: absolute;
                    top: 0px;
                    left: 0px;
                    color: #ffffff;
                    background-color: rgba(255, 119, 0, .7);
                    text-align: center;
                    font-size: 11px;
                    font-style: normal;
                }
                img {
                    width: 140px;
                    overflow: hidden;
                }
            }
            >.media-body {
                >h5 {
                    color: #333333;
                    margin-top: -3px;
                    margin-bottom: 5px;
                    font-size: 15px;
                    overflow: hidden;
                    line-height: 28px;
                    max-height: 56px;
                    text-align: left;
                    text-align-last: left;
                    -webkit-line-clamp: 2;
                    display: -webkit-box;
                    -webkit-box-orient: vertical;
                }
                >p {
                    color: #f40;
                    margin-top: 2px;
                    font-size: 16px;
                    line-height: 28px;
                    height: 28px;
                    overflow: hidden;
                    -webkit-line-clamp: 1;
                    display: -webkit-box;
                    -webkit-box-orient: vertical;
                }
                .dress {
                    position: absolute;
                    bottom: -12px;
                    color: #ccc;
                    font-size: 12px;
                }
            }
        }
    }
    .pagenavi {
        height: 44px;
        padding: 0px 20%;
        box-sizing: border-box;
        position: fixed;
        bottom: 0px;
        left: 0px;
        right: 0px;
        background-color: #fff;
        border-top: 1px solid #ccc;
        a.prev,
        a.next {
            display: block;
            float: left;
            width: 50%;
            line-height: 44px;
            color: #ff7700;
            text-align: center;
            font-size: 16px;
            font-weight: bold;
            box-sizing: border-box;
        }
        a.disabled {
            color: #999999;
        }
    }
    >.nav {
        height: 36px;
        background-color: #ebebeb;
        background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#ebebeb));
        background: -moz-linear-gradient(top, #ffffff, #ebebeb);
        background: -o-linear-gradient(top, #ffffff, #ebebeb);
        border-top: solid 1px #ebebeb;
        border-bottom: 1px solid #ccc;
        line-height: 36px;
        padding-left: 8px;
        font-size: 14px;
        margin-top: 20px;
    }
    >#footer {
        background: #efefef;
        text-align: center;
        font-size: 12px;
        height: 90px; // padding: 15px 0;
        color: #666;
        >.foot_top {
            background: #efefef;
            border-top: 1px solid #c4c4c4;
            border-bottom: 1px solid #c4c4c4;
            height: 34px;
            line-height: 34px;
            margin-top: 20px;
            clear: both;
            >a {
                -webkit-box-sizing: border-box;
                -moz-box-sizing: border-box;
                -o-box-sizing: border-box;
                box-sizing: border-box;
                float: left;
                font-size: 14px;
                border-right: 1px solid #c4c4c4;
                text-align: center;
                width: 30%;
                >img {
                    vertical-align: middle;
                    margin-top: -5px;
                    margin-right: 2px;
                }
            }
        }
    }
}