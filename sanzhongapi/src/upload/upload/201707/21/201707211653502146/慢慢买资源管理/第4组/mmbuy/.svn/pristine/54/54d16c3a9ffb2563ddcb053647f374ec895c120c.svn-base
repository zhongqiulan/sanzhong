    // 渲染list模板样式
    .bcj_list_ul,.bcj_list_ul2{
        .media {
            width:100%;
            margin: 0;
            margin-bottom: 10px;
            padding: 15px 8px;
            background-color: #fff;            
            .cont_img {
                float: left;
                margin-left: 10px;
                // background-image: url(https://img.alicdn.com/bao/uploaded/i4/TB1_luSNVXXXXcBXXXXXXXXXXXX_!!0-item_pic.jpg_430x430q90.jpg);
                img {
                    width:130px;
                    height:130px;
                    
                }
            }
            .con_con {                
                margin-left: 15px;  
                width:200px;
                float:left; 
                padding-top:10px;
                           
                
                >h4 {
                    font-size: 16px;
                    margin: 0;
                    height: 20px;
                    line-height: 20px;
                    width:100%;
                    overflow: hidden;                    
                }
                >p {
                    margin: 0;
                    height: 24px;
                    line-height: 24px;
                    margin-top:10px;
                    font-size: 20px;
                    color: #fc353a;
                    i,
                    em,
                    del {
                        font-size: 20px;
                        padding-left: 12px;
                    }
                    del {
                        color: #b2b2b2;
                    }
                }
                .bar {
                    font-size: 10px;
                    color: #fc353a;
                    padding: 0 0;
                    >i {
                        text-align: center;
                        float: left;
                        height: 20px;
                        line-height: 20px;
                        background-color: #ffb7b7;
                        width: 150px;
                        border-radius: 50px;
                        color: #fff;
                    }
                    >b {
                        float: left;
                        margin-left: 20px;
                        height: 20px;
                        line-height: 20px;
                    }
                }
                .bottom {
                    font-size: 20px;
                    margin-top:20px;
                    .l {
                        float: left;
                        margin-top: 10px;
                        padding: 0;
                        border: 1px dashed #fc353a;
                        color: #fc353a;
                    }
                    .r {
                        float: right;
                        padding: 10px;
                        background-color: #fc353a;
                        color: #fff;
                        border-radius: 10px;
                        a{
                            color:#FFF
                        }
                    }
                }
            }
        }
    }