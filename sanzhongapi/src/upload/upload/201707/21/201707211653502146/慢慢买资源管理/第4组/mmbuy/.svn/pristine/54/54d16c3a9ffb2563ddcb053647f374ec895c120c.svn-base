    * {
        font-size: 14px;
        text-align: center;
    }

    .hide {
        display: none;
    }

    .show {
        display: block;
    }

    .border {
        border-bottom: 2px solid #e4393c;
    }

    .layout {
        width: 100%;
        header {
            background-color: #e4393c;
            height: 44px;
            color: #FFF;
            position: relative;
            line-height: 44px;
            padding: 0 10px;
            a {
                color: #FFF;
                position: absolute;
                font-size: 20px;
                left: 0;
            }
            h2 {
                font-size: 20px;
                display: inline-block;
                line-height: 20px;
                margin-top:10px;
            }
            i{
                position: absolute;
                right:0;
                top:-10px;
                font-size:36px;
            }
        }
        .bcj_head {
            position: relative;
            width: 100%;
            height: 36px;
            line-height: 36px;
            .searchBox {
                width: 100%;
                height: 55px;
                padding: 10px 0;
                background: #f7f7f7;
                >div {
                    width: 100%;
                    height: 35px;
                    position: relative;
                    .input-find {
                        position: absolute;
                        height: 35px;
                        width: 70%;
                        left: 10px;
                        border: none;
                    }
                    .input-submit {
                        position: absolute;
                        height: 35px;
                        width: 30%;
                        right: 10px;
                        background: #f7f7f7;
                    }
                }
            }
            .bcj_ul {
                overflow: hidden;
                width: 100%;
                padding-right: 38px;
                ul {
                    margin-left: -15px;
                    li {
                        display: inline-block;
                        margin: 0 12px;
                    }
                }
            }
            .searchBtn {
                position: absolute;
                height: 38px;
                width: 38px;
                right: 0;
                top: 0;
                border-left: 1px solid #e6e6e6;
                background-color: #fbfbfb;
                .icon-search2 {
                    position: absolute;
                    top:-10px;
                    left:0;
                    display: inline-block;
                    font-size: 30px;
                    line-height: 38px;
                }
            }
            .searchBox {}
        }
        .nav_discount {
            width: 100%;
            background-color: #fbfbfb;
            img {
                width: 100%;
                margin: 10px 0
            }
        }
        .img_loading {
            position: relative;
            height: 70px;
            img {
                position: absolute;
                top: 50%;
                left: 50%;
                margin-top: -16px;
                margin-left: -16px;
                width: 32px;
                height: 32px;
            }
        }
        .list_bottom {
            text-align: left;
            padding-left: 10px;
            height: 38px;
            line-height: 38px;
            background-color: #ebebeb;
            border-bottom: solid 1px #ebebeb;
            border-top: solid 1px #ccc;
            margin-top: 20px;
            >a {
                display: inline-block
            }
        }
        .app_adv {
            position: relative;
            width: 100%;
            height: 100px;
            background-color: #434343;
            padding: 10px;
            margin-top: 20px;
            img {
                position: absolute;
                top: 10px;
                left: 10px;
                width: 64px;
                height: 64px;
            }
            .app_text {
                position: absolute;
                top: 10px;
                left: 84px;
                h4 {
                    text-align: left;
                    color: #FFF;
                    font-size: 18px;
                    font-weight: normal;
                    height: 36px;
                    line-height: 36px;
                    margin-top:0;
                }
                p {
                    text-align: left;
                    color: #fee6c2;
                }
            }
            .app_button {
                right: 10px;
                top: 10px;
                position: absolute;
                background: #6db110;
                height: 30px;
                width: 80px;
                line-height: 30px;
                border-radius: 4px;
                a {
                    color: #FFF;
                }
            }
        }
    }

    .bcj_list_ul,.bcj_list_ul2{
        .ad_pic {
            float: left;
            width: 135px;
            height: 130px;
            img {
                width: 130px;
                height: 130px;
            }
        }
        li {
            height: 150px;
            width: 100%;
        }
    }
    


